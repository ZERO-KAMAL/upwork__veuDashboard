n      this.$toast({\r\n        component: ToastificationContent,\r\n        props: {\r\n          title: 'Notification',\r\n          icon: 'InfoIcon',\r\n          text: 'I do not think that word means what you think it means.',\r\n          variant,\r\n        },\r\n      },\r\n      {\r\n        position,\r\n      })\r\n    },\r\n  },\r\n}\r\n</script>\r\n`\r\n\r\nexport const codeTime = `\r\n<template>\r\n  <div class=\"demo-inline-spacing\">\r\n\r\n    <!-- default time trigger button -->\r\n    <b-button\r\n      v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n      variant=\"outline-primary\"\r\n      @click=\"showToast('primary')\"\r\n    >\r\n      Time Default\r\n    </b-button>\r\n\r\n    <!-- 4s delay trigger button -->\r\n    <b-button\r\n      v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n      variant=\"outline-primary\"\r\n      @click=\"showToast('primary',4000)\"\r\n    >\r\n      Time 4s(4000)\r\n    </b-button>\r\n\r\n    <!-- 8s delay trigger button -->\r\n    <b-button\r\n      v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n      variant=\"outline-primary\"\r\n      @click=\"showToast('primary',8000)\"\r\n    >\r\n      Time 8s(8000)\r\n    </b-button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { BButton} from 'bootstrap-vue'\r\nimport ToastificationContent from '@core/components/toastification/ToastificationContent.vue'\r\nimport Ripple from 'vue-ripple-directive'\r\n\r\nexport default {\r\n  components: {\r\n    BButton,\r\n    BCardText,\r\n    // eslint-disable-next-line vue/no-unused-components\r\n    ToastificationContent,\r\n  },\r\n  directives: {\r\n    Ripple,\r\n  },\r\n  methods: {\r\n    showToast(variant, timeout) {\r\n      this.$toast({\r\n        component: ToastificationContent,\r\n        props: {\r\n          title: 'Notification',\r\n          icon: 'BellIcon',\r\n          text: 'I do not think that word means what you think it means.',\r\n          variant,\r\n        },\r\n      },\r\n      {\r\n        timeout,\r\n      })\r\n    },\r\n  },\r\n}\r\n</script>\r\n`\r\n\r\nexport const codeIcon = `\r\n<template>\r\n  <div class=\"demo-inline-spacing\">\r\n    <!-- mail icon -->\r\n    <b-button\r\n      v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n      variant=\"outline-primary\"\r\n      @click=\"showToast('primary','MailIcon')\"\r\n    >\r\n      Icon Mail\r\n    </b-button>\r\n\r\n    <!-- Inbox icon -->\r\n    <b-button\r\n      v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n      variant=\"outline-primary\"\r\n      @click=\"showToast('secondary','InboxIcon')\"\r\n    >\r\n      Icon Inbox\r\n    </b-button>\r\n\r\n    <!-- Check icon -->\r\n    <b-button\r\n      v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n      variant=\"outline-primary\"\r\n      @click=\"showToast('success','CheckIcon')\"\r\n    >\r\n      Icon Check\r\n    </b-button>\r\n\r\n    <!-- heart icon -->\r\n    <b-button\r\n      v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n      variant=\"outline-primary\"\r\n      @click=\"showToast('danger','HeartIcon')\"\r\n    >\r\n      Icon Heart\r\n    </b-button>\r\n\r\n    <!-- favorite icon -->\r\n    <b-button\r\n      v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n      variant=\"outline-primary\"\r\n      @click=\"showToast('warning','StarIcon')\"\r\n    >\r\n      Icon Favorite\r\n    </b-button>\r\n\r\n    <!-- slack icon -->\r\n    <b-button\r\n      v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n      variant=\"outline-primary\"\r\n      @click=\"showToast('info','SlackIcon')\"\r\n    >\r\n      Icon Slack\r\n    </b-button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { BButton } from 'bootstrap-vue'\r\nimport ToastificationContent from '@core/components/toastification/ToastificationContent.vue'\r\nimport Ripple from 'vue-ripple-directive'\r\n\r\nexport default {\r\n  components: {\r\n    BButton,\r\n    BCardCode,\r\n    // eslint-disable-next-line vue/no-unused-components\r\n    ToastificationContent,\r\n  },\r\n  directive