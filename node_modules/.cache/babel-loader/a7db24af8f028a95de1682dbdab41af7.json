AANA;AAXA,CAAA","sourcesContent":["<template>\r\n  <b-card-code title=\"File name formatter function\">\r\n    <b-card-text>\r\n      <span>Set the prop </span>\r\n      <code>file-name-formatter</code>\r\n      <span> to a function that accepts a single argument which is an array of File objects.</span>\r\n    </b-card-text>\r\n\r\n    <b-form-file\r\n      multiple\r\n      :file-name-formatter=\"formatNames\"\r\n    />\r\n\r\n    <template #code>\r\n      {{ codeFormatter }}\r\n    </template>\r\n  </b-card-code>\r\n</template>\r\n\r\n<script>\r\nimport BCardCode from '@core/components/b-card-code'\r\nimport { BFormFile, BCardText } from 'bootstrap-vue'\r\nimport { codeFormatter } from './code'\r\n\r\nexport default {\r\n  components: {\r\n    BCardCode,\r\n    BFormFile,\r\n    BCardText,\r\n  },\r\n  data() {\r\n    return {\r\n      codeFormatter,\r\n    }\r\n  },\r\n  methods: {\r\n    formatNames(files) {\r\n      if (files.length === 1) {\r\n        return files[0].name\r\n      }\r\n      return `${files.length} files selected`\r\n    },\r\n  },\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/forms/form-element/form-file-input"}]}