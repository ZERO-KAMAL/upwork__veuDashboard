,KADA;AACA,IAAA,IAAA,EAAA,IADA;AACA,IAAA,OAAA,EAAA,OADA;AACA,IAAA,MAAA,EAAA,MADA;AACA,IAAA,eAAA,EAAA;AADA,GADA;AAIA,EAAA,KAJA,mBAIA;AACA,QAAA,QAAA,GAAA,GAAA,CAAA,EAAA,CAAA;;AAEA,0BAAA,cAAA,EAAA;AAAA,QAAA,uBAAA,mBAAA,uBAAA;;AACA,wBAAA,YAAA,EAAA;AAAA,QAAA,qBAAA,iBAAA,qBAAA;;AACA,QAAA,0BAAA,GAAA,SAAA,0BAAA,CAAA,OAAA,EAAA;AACA,MAAA,qBAAA,CAAA,OAAA,CAAA,EAAA,CAAA,CACA,IADA,CACA,YAAA;AACA,YAAA,YAAA,GAAA,QAAA,CAAA,KAAA,CAAA,SAAA,CAAA,UAAA,CAAA;AAAA,iBAAA,CAAA,CAAA,EAAA,KAAA,OAAA,CAAA,EAAA;AAAA,SAAA,CAAA;AACA,QAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAAA,YAAA,EAAA,CAAA;AAEA,QAAA,KAAA,CAAA,MAAA,CAAA,uCAAA,EAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA,OANA;AAOA,KARA;;AAUA,QAAA,iBAAA,GAAA,SAAA,iBAAA,GAAA;AACA,MAAA,KAAA,CAAA,QAAA,CAAA,iCAAA,EACA,IADA,CACA,UAAA,QAAA,EAAA;AACA,QAAA,QAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA,CAAA,QAAA;AACA,OAHA;AAIA,KALA;;AAMA,IAAA,iBAAA;AAEA,WAAA;AACA,MAAA,QAAA,EAAA,QADA;AAGA;AACA,MAAA,uBAAA,EAAA,uBAJA;AAKA,MAAA,0BAAA,EAAA,0BALA;AAOA;AACA,MAAA,UAAA,EAAA;AARA,KAAA;AAUA;AArCA,CAAA","sourcesContent":["<template>\r\n\r\n  <div class=\"checkout-items\">\r\n    <b-card\r\n      v-for=\"product in products\"\r\n      :key=\"product.id\"\r\n      class=\"ecommerce-card\"\r\n      no-body\r\n    >\r\n\r\n      <!-- Product Image -->\r\n      <div class=\"item-img\">\r\n        <b-link>\r\n          <b-img\r\n            :src=\"product.image\"\r\n            :alt=\"`${product.name}-${product.id}`\"\r\n          />\r\n        </b-link>\r\n      </div>\r\n\r\n      <!-- Product Details: Card Body -->\r\n      <b-card-body>\r\n        <div class=\"item-name\">\r\n          <h6 class=\"mb-0\">\r\n            <b-link class=\"text-body\">\r\n              {{ product.name }}\r\n            </b-link>\r\n          </h6>\r\n          <span class=\"item-company\">By <b-link class=\"company-name\">{{ product.brand }}</b-link></span>\r\n          <div class=\"item-rating\">\r\n            <ul class=\"unstyled-list list-inline\">\r\n              <li\r\n                v-for=\"star in 5\"\r\n                :key=\"star\"\r\n                class=\"ratings-list-item\"\r\n                :class=\"{'ml-25': star-1}\"\r\n              >\r\n                <feather-icon\r\n                  icon=\"StarIcon\"\r\n                  size=\"16\"\r\n                  :class=\"[{'fill-current': star <= product.rating}, star <= product.rating ? 'text-warning' : 'text-muted']\"\r\n                />\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n        <span class=\"text-success mb-1\">In Stock</span>\r\n        <div class=\"item-quantity\">\r\n          <span class=\"quantity-title\">Qty:</span>\r\n          <b-form-spinbutton\r\n            v-model=\"product.qty\"\r\n            size=\"sm\"\r\n            class=\"ml-75\"\r\n            inline\r\n          />\r\n        </div>\r\n        <span class=\"delivery-date text-muted\">Delivery by {{ formatDate(product.shippingDate, { month: 'short', day: 'numeric', weekday: 'short' }) }}</span>\r\n        <span class=\"text-success\">{{ product.discountPercentage }}% off {{ product.offers }} offers Available</span>\r\n      </b-card-body>\r\n\r\n      <!-- Product Options/Actions -->\r\n      <div class=\"item-options text-center\">\r\n        <div class=\"item-wrapper\">\r\n          <div class=\"item-cost\">\r\n            <h4 class=\"item-price\">\r\n              ${{ product.price }}\r\n            </h4>\r\n            <p\r\n              v-if=\"product.hasFreeShipping\"\r\n              class=\"card-text shipping\"\r\n            >\r\n              <b-badge\r\n                pill\r\n                variant=\"light-success\"\r\n              >\r\n                Free Shipping\r\n              </b-badge>\r\n            </p>\r\n          </div>\r\n        </div>\r\n        <b-button\r\n          variant=\"light\"\r\n          class=\"mt-1 remove-wishlist\"\r\n          @click=\"removeProductFromCartClick(product)\"\r\n        >\r\n          <feather-icon\r\n            icon=\"XIcon\"\r\n            class=\"mr-50\"\r\n          />\r\n          <span>Remove</span>\r\n        </b-button>\r\n        <b-button\r\n          variant=\"primary\"\r\n          class=\"btn-cart move-cart\"\r\n          @click=\"toggleProductInWishlist(product)\"\r\n        >\r\n          <feather-icon\r\n            icon=\"HeartIcon\"\r\n            class=\"mr-50\"\r\n            :class=\"{'fill-current': product.isInWishlist}\"\r\n          />\r\n          <span class=\"text-nowrap\">Wishlist</span>\r\n        </b-button>\r\n      </div>\r\n    </b-card>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  BCard, BCardBody, BLink, BImg, BButton, BBadge, BFormSpinbutton,\r\n} from 'bootstrap-vue'\r\nimport store from '@/store'\r\nimport { ref } from '@vue/composition-api'\r\nimport { formatDate } from '@core/utils/filter'\r\nimport { useEcommerce, useEcommerceUi } from '../useEcommerce'\r\n\r\nexport default {\r\n  components: {\r\n    BCard, BCardBody, BLink, BImg, BButton, BBadge, BFormSpinbutton,\r\n  },\r\n  setup() {\r\n    const products = ref([])\r\n\r\n    const { toggleProductInWishlist } = useEcommerceUi()\r\n    const { removeProductFromCart } = useEcommerce()\r\n    const removeProductFromCartClick = product => {\r\n      removeProductFromCart(product.id)\r\n        .then(() => {\r\n          const productIndex = products.value.findIndex(p => p.id === product.id)\r\n          products.value.splice(productIndex, 1)\r\n\r\n          store.commit('app-ecommerce/UPDATE_CART_ITEMS_COUNT', products.value.length)\r\n        })\r\n    }\r\n\r\n    const fetchCartProducts = () => {\r\n      store.dispatch('app-ecommerce/fetchCartProducts')\r\n        .then(response => {\r\n          products.value = response.data.products\r\n        })\r\n    }\r\n    fetchCartProducts()\r\n\r\n    return {\r\n      products,\r\n\r\n      // UI\r\n      toggleProductInWishlist,\r\n      removeProductFromCartClick,\r\n\r\n      // Filter\r\n      formatDate,\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"],"sourceRoot":"src/views/apps/e-commerce/e-commerce-checkout"}]}