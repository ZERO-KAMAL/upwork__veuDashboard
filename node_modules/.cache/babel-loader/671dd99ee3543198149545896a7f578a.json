,QAAA,sBAAA;AACA,OAAA,KAAA,MAAA,SAAA;AACA,OAAA,eAAA,MAAA,kBAAA;AAEA,OAAA,kBAAA,MAAA,uBAAA;AAEA,eAAA;AACA,EAAA,UAAA,EAAA;AACA,IAAA,KAAA,EAAA,KADA;AAEA,IAAA,IAAA,EAAA,IAFA;AAGA,IAAA,IAAA,EAAA,IAHA;AAIA,IAAA,UAAA,EAAA,UAJA;AAKA,IAAA,OAAA,EAAA,OALA;AAMA,IAAA,MAAA,EAAA,MANA;AAOA,IAAA,MAAA,EAAA,MAPA;AAQA,IAAA,OAAA,EAAA,OARA;AASA,IAAA,KAAA,EAAA,KATA;AAUA,IAAA,MAAA,EAAA,MAVA;AAWA,IAAA,SAAA,EAAA,SAXA;AAYA,IAAA,aAAA,EAAA,aAZA;AAaA,IAAA,WAAA,EAAA,WAbA;AAcA,IAAA,QAAA,EAAA,QAdA;AAgBA,IAAA,OAAA,EAAA;AAhBA,GADA;AAmBA,EAAA,KAnBA,mBAmBA;AACA,QAAA,6BAAA,GAAA,aAAA,CADA,CAGA;;AACA,QAAA,CAAA,KAAA,CAAA,SAAA,CAAA,6BAAA,CAAA,EAAA,KAAA,CAAA,cAAA,CAAA,6BAAA,EAAA,kBAAA,EAJA,CAMA;;AACA,IAAA,WAAA,CAAA,YAAA;AACA,UAAA,KAAA,CAAA,SAAA,CAAA,6BAAA,CAAA,EAAA,KAAA,CAAA,gBAAA,CAAA,6BAAA;AACA,KAFA,CAAA;AAIA,QAAA,aAAA,GAAA,CACA,YADA,EAEA,OAFA,EAGA,MAHA,EAIA,iBAJA,EAKA,UALA,CAAA;;AAQA,2BAmBA,eAAA,EAnBA;AAAA,QACA,aADA,oBACA,aADA;AAAA,QAEA,YAFA,oBAEA,YAFA;AAAA,QAGA,OAHA,oBAGA,OAHA;AAAA,QAIA,WAJA,oBAIA,WAJA;AAAA,QAKA,aALA,oBAKA,aALA;AAAA,QAMA,QANA,oBAMA,QANA;AAAA,QAOA,cAPA,oBAOA,cAPA;AAAA,QAQA,WARA,oBAQA,WARA;AAAA,QASA,MATA,oBASA,MATA;AAAA,QAUA,aAVA,oBAUA,aAVA;AAAA,QAWA,mBAXA,oBAWA,mBAXA;AAAA,QAaA,YAbA,oBAaA,YAbA;AAAA,QAeA,WAfA,oBAeA,WAfA;AAAA,QAiBA,kCAjBA,oBAiBA,kCAjBA;AAAA,QAkBA,0BAlBA,oBAkBA,0BAlBA;;AAqBA,WAAA;AACA,MAAA,aAAA,EAAA,aADA;AAEA,MAAA,YAAA,EAAA,YAFA;AAGA,MAAA,OAAA,EAAA,OAHA;AAIA,MAAA,WAAA,EAAA,WAJA;AAKA,MAAA,aAAA,EAAA,aALA;AAMA,MAAA,QAAA,EAAA,QANA;AAOA,MAAA,cAAA,EAAA,cAPA;AAQA,MAAA,WAAA,EAAA,WARA;AASA,MAAA,MAAA,EAAA,MATA;AAUA,MAAA,aAAA,EAAA,aAVA;AAWA,MAAA,mBAAA,EAAA,mBAXA;AAaA,MAAA,YAAA,EAAA,YAbA;AAeA,MAAA,WAAA,EAAA,WAfA;AAiBA,MAAA,aAAA,EAAA,aAjBA;AAmBA,MAAA,UAAA,EAAA,UAnBA;AAoBA,MAAA,kCAAA,EAAA,kCApBA;AAqBA,MAAA,0BAAA,EAAA;AArBA,KAAA;AAuBA;AAlFA,CAAA","sourcesContent":["<template>\r\n\r\n  <!-- Table Container Card -->\r\n  <b-card\r\n    no-body\r\n  >\r\n\r\n    <div class=\"m-2\">\r\n\r\n      <!-- Table Top -->\r\n      <b-row>\r\n\r\n        <!-- Per Page -->\r\n        <b-col\r\n          cols=\"12\"\r\n          md=\"6\"\r\n          class=\"d-flex align-items-center justify-content-start mb-1 mb-md-0\"\r\n        >\r\n          <label>Entries</label>\r\n          <v-select\r\n            v-model=\"perPage\"\r\n            :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\r\n            :options=\"perPageOptions\"\r\n            :clearable=\"false\"\r\n            class=\"per-page-selector d-inline-block ml-50 mr-1\"\r\n          />\r\n          <b-button\r\n            variant=\"primary\"\r\n            :to=\"{ name: 'apps-invoice-add'}\"\r\n          >\r\n            Add Record\r\n          </b-button>\r\n        </b-col>\r\n\r\n        <!-- Search -->\r\n        <b-col\r\n          cols=\"12\"\r\n          md=\"6\"\r\n        >\r\n          <div class=\"d-flex align-items-center justify-content-end\">\r\n            <b-form-input\r\n              v-model=\"searchQuery\"\r\n              class=\"d-inline-block mr-1\"\r\n              placeholder=\"Search...\"\r\n            />\r\n            <v-select\r\n              v-model=\"statusFilter\"\r\n              :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\r\n              :options=\"statusOptions\"\r\n              class=\"invoice-filter-select\"\r\n              placeholder=\"Select Status\"\r\n            >\r\n              <template #selected-option=\"{ label }\">\r\n                <span class=\"text-truncate overflow-hidden\">\r\n                  {{ label }}\r\n                </span>\r\n              </template>\r\n            </v-select>\r\n          </div>\r\n        </b-col>\r\n      </b-row>\r\n\r\n    </div>\r\n\r\n    <b-table\r\n      ref=\"refInvoiceListTable\"\r\n      :items=\"fetchInvoices\"\r\n      responsive\r\n      :fields=\"tableColumns\"\r\n      primary-key=\"id\"\r\n      :sort-by.sync=\"sortBy\"\r\n      show-empty\r\n      empty-text=\"No matching records found\"\r\n      :sort-desc.sync=\"isSortDirDesc\"\r\n      class=\"position-relative\"\r\n    >\r\n\r\n      <template #head(invoiceStatus)>\r\n        <feather-icon\r\n          icon=\"TrendingUpIcon\"\r\n          class=\"mx-auto\"\r\n        />\r\n      </template>\r\n\r\n      <!-- Column: Id -->\r\n      <template #cell(id)=\"data\">\r\n        <b-link\r\n          :to=\"{ name: 'apps-invoice-preview', params: { id: data.item.id }}\"\r\n          class=\"font-weight-bold\"\r\n        >\r\n          #{{ data.value }}\r\n        </b-link>\r\n      </template>\r\n\r\n      <!-- Column: Invoice Status -->\r\n      <template #cell(invoiceStatus)=\"data\">\r\n        <b-avatar\r\n          :id=\"`invoice-row-${data.item.id}`\"\r\n          size=\"32\"\r\n          :variant=\"`light-${resolveInvoiceStatusVariantAndIcon(data.item.invoiceStatus).variant}`\"\r\n        >\r\n          <feather-icon\r\n            :icon=\"resolveInvoiceStatusVariantAndIcon(data.item.invoiceStatus).icon\"\r\n          />\r\n        </b-avatar>\r\n        <b-tooltip\r\n          :target=\"`invoice-row-${data.item.id}`\"\r\n          placement=\"top\"\r\n        >\r\n          <p class=\"mb-0\">\r\n            {{ data.item.invoiceStatus }}\r\n          </p>\r\n          <p class=\"mb-0\">\r\n            Balance: {{ data.item.balance }}\r\n          </p>\r\n          <p class=\"mb-0\">\r\n            Due Date: {{ data.item.dueDate }}\r\n          </p>\r\n        </b-tooltip>\r\n      </template>\r\n\r\n      <!-- Column: Client -->\r\n      <template #cell(client)=\"data\">\r\n        <b-media vertical-align=\"center\">\r\n          <template #aside>\r\n            <b-avatar\r\n              size=\"32\"\r\n              :src=\"data.item.avatar\"\r\n              :text=\"avatarText(data.item.client.name)\"\r\n              :variant=\"`light-${resolveClientAvatarVariant(data.item.invoiceStatus)}`\"\r\n            />\r\n          </template>\r\n          <span class=\"font-weight-bold d-block text-nowrap\">\r\n            {{ data.item.client.name }}\r\n          </span>\r\n          <small class=\"text-muted\">{{ data.item.client.companyEmail }}</small>\r\n        </b-media>\r\n      </template>\r\n\r\n      <!-- Column: Issued Date -->\r\n      <template #cell(issuedDate)=\"data\">\r\n        <span class=\"text-nowrap\">\r\n          {{ data.value }}\r\n        </span>\r\n      </template>\r\n\r\n      <!-- Column: Balance -->\r\n      <template #cell(balance)=\"data\">\r\n        <template v-if=\"data.value === 0\">\r\n          <b-badge\r\n            pill\r\n            variant=\"light-success\"\r\n          >\r\n            Paid\r\n          </b-badge>\r\n        </template>\r\n        <template v-else>\r\n          {{ data.value }}\r\n        </template>\r\n      </template>\r\n\r\n      <!-- Column: Actions -->\r\n      <template #cell(actions)=\"data\">\r\n\r\n        <div class=\"text-nowrap\">\r\n          <feather-icon\r\n            :id=\"`invoice-row-${data.item.id}-send-icon`\"\r\n            icon=\"SendIcon\"\r\n            class=\"cursor-pointer\"\r\n            size=\"16\"\r\n          />\r\n          <b-tooltip\r\n            title=\"Send Invoice\"\r\n            class=\"cursor-pointer\"\r\n            :target=\"`invoice-row-${data.item.id}-send-icon`\"\r\n          />\r\n\r\n          <feather-icon\r\n            :id=\"`invoice-row-${data.item.id}-preview-icon`\"\r\n            icon=\"EyeIcon\"\r\n            size=\"16\"\r\n            class=\"mx-1\"\r\n            @click=\"$router.push({ name: 'apps-invoice-preview', params: { id: data.item.id }})\"\r\n          />\r\n          <b-tooltip\r\n            title=\"Preview Invoice\"\r\n            :target=\"`invoice-row-${data.item.id}-preview-icon`\"\r\n          />\r\n\r\n          <!-- Dropdown -->\r\n          <b-dropdown\r\n            variant=\"link\"\r\n            toggle-class=\"p-0\"\r\n            no-caret\r\n            :right=\"$store.state.appConfig.isRTL\"\r\n          >\r\n\r\n            <template #button-content>\r\n              <feather-icon\r\n                icon=\"MoreVerticalIcon\"\r\n                size=\"16\"\r\n                class=\"align-middle text-body\"\r\n              />\r\n            </template>\r\n            <b-dropdown-item>\r\n              <feather-icon icon=\"DownloadIcon\" />\r\n              <span class=\"align-middle ml-50\">Download</span>\r\n            </b-dropdown-item>\r\n            <b-dropdown-item :to=\"{ name: 'apps-invoice-edit', params: { id: data.item.id } }\">\r\n              <feather-icon icon=\"EditIcon\" />\r\n              <span class=\"align-middle ml-50\">Edit</span>\r\n            </b-dropdown-item>\r\n            <b-dropdown-item>\r\n              <feather-icon icon=\"TrashIcon\" />\r\n              <span class=\"align-middle ml-50\">Delete</span>\r\n            </b-dropdown-item>\r\n            <b-dropdown-item>\r\n              <feather-icon icon=\"CopyIcon\" />\r\n              <span class=\"align-middle ml-50\">Duplicate</span>\r\n            </b-dropdown-item>\r\n          </b-dropdown>\r\n        </div>\r\n      </template>\r\n\r\n    </b-table>\r\n    <div class=\"mx-2 mb-2\">\r\n      <b-row>\r\n\r\n        <b-col\r\n          cols=\"12\"\r\n          sm=\"6\"\r\n          class=\"d-flex align-items-center justify-content-center justify-content-sm-start\"\r\n        >\r\n          <span class=\"text-muted\">Showing {{ dataMeta.from }} to {{ dataMeta.to }} of {{ dataMeta.of }} entries</span>\r\n        </b-col>\r\n        <!-- Pagination -->\r\n        <b-col\r\n          cols=\"12\"\r\n          sm=\"6\"\r\n          class=\"d-flex align-items-center justify-content-center justify-content-sm-end\"\r\n        >\r\n\r\n          <b-pagination\r\n            v-model=\"currentPage\"\r\n            :total-rows=\"totalInvoices\"\r\n            :per-page=\"perPage\"\r\n            first-number\r\n            last-number\r\n            class=\"mb-0 mt-1 mt-sm-0\"\r\n            prev-class=\"prev-item\"\r\n            next-class=\"next-item\"\r\n          >\r\n            <template #prev-text>\r\n              <feather-icon\r\n                icon=\"ChevronLeftIcon\"\r\n                size=\"18\"\r\n              />\r\n            </template>\r\n            <template #next-text>\r\n              <feather-icon\r\n                icon=\"ChevronRightIcon\"\r\n                size=\"18\"\r\n              />\r\n            </template>\r\n          </b-pagination>\r\n\r\n        </b-col>\r\n\r\n      </b-row>\r\n    </div>\r\n  </b-card>\r\n\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  BCard, BRow, BCol, BFormInput, BButton, BTable, BMedia, BAvatar, BLink,\r\n  BBadge, BDropdown, BDropdownItem, BPagination, BTooltip,\r\n} from 'bootstrap-vue'\r\nimport { avatarText } from '@core/utils/filter'\r\nimport vSelect from 'vue-select'\r\nimport { onUnmounted } from '@vue/composition-api'\r\nimport store from '@/store'\r\nimport useInvoicesList from './useInvoiceList'\r\n\r\nimport invoiceStoreModule from '../invoiceStoreModule'\r\n\r\nexport default {\r\n  components: {\r\n    BCard,\r\n    BRow,\r\n    BCol,\r\n    BFormInput,\r\n    BButton,\r\n    BTable,\r\n    BMedia,\r\n    BAvatar,\r\n    BLink,\r\n    BBadge,\r\n    BDropdown,\r\n    BDropdownItem,\r\n    BPagination,\r\n    BTooltip,\r\n\r\n    vSelect,\r\n  },\r\n  setup() {\r\n    const INVOICE_APP_STORE_MODULE_NAME = 'app-invoice'\r\n\r\n    // Register module\r\n    if (!store.hasModule(INVOICE_APP_STORE_MODULE_NAME)) store.registerModule(INVOICE_APP_STORE_MODULE_NAME, invoiceStoreModule)\r\n\r\n    // UnRegister on leave\r\n    onUnmounted(() => {\r\n      if (store.hasModule(INVOICE_APP_STORE_MODULE_NAME)) store.unregisterModule(INVOICE_APP_STORE_MODULE_NAME)\r\n    })\r\n\r\n    const statusOptions = [\r\n      'Downloaded',\r\n      'Draft',\r\n      'Paid',\r\n      'Partial Payment',\r\n      'Past Due',\r\n    ]\r\n\r\n    const {\r\n      fetchInvoices,\r\n      tableColumns,\r\n      perPage,\r\n      currentPage,\r\n      totalInvoices,\r\n      dataMeta,\r\n      perPageOptions,\r\n      searchQuery,\r\n      sortBy,\r\n      isSortDirDesc,\r\n      refInvoiceListTable,\r\n\r\n      statusFilter,\r\n\r\n      refetchData,\r\n\r\n      resolveInvoiceStatusVariantAndIcon,\r\n      resolveClientAvatarVariant,\r\n    } = useInvoicesList()\r\n\r\n    return {\r\n      fetchInvoices,\r\n      tableColumns,\r\n      perPage,\r\n      currentPage,\r\n      totalInvoices,\r\n      dataMeta,\r\n      perPageOptions,\r\n      searchQuery,\r\n      sortBy,\r\n      isSortDirDesc,\r\n      refInvoiceListTable,\r\n\r\n      statusFilter,\r\n\r\n      refetchData,\r\n\r\n      statusOptions,\r\n\r\n      avatarText,\r\n      resolveInvoiceStatusVariantAndIcon,\r\n      resolveClientAvatarVariant,\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.per-page-selector {\r\n  width: 90px;\r\n}\r\n\r\n.invoice-filter-select {\r\n  min-width: 190px;\r\n\r\n  ::v-deep .vs__selected-options {\r\n    flex-wrap: nowrap;\r\n  }\r\n\r\n  ::v-deep .vs__selected {\r\n    width: 100px;\r\n  }\r\n}\r\n</style>\r\n\r\n<style lang=\"scss\">\r\n@import '@core/scss/vue/libs/vue-select.scss';\r\n</style>\r\n"],"sourceRoot":"src/views/apps/invoice/invoice-list"}]}