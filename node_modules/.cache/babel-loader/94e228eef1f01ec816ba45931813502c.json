template>\r\n  <b-card-code title=\"Overlay corner rounding\">\r\n    <b-card-text>\r\n      <span>\r\n        By default, the overlay backdrop has square corners. If the content you are wrapping has rounded corners, you\r\n        can use the\r\n      </span>\r\n      <code>rounded</code>\r\n      <span> prop to apply rounding to the overlay's corners to match the obscured content's rounded corners.</span>\r\n    </b-card-text>\r\n\r\n    <div>\r\n      <b-button\r\n        v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\r\n        variant=\"primary\"\r\n        @click=\"show = !show\"\r\n      >\r\n        Toggle overlay\r\n      </b-button>\r\n\r\n      <b-row class=\"text-center mt-2\">\r\n        <b-col md=\"6\">\r\n          <b-card-text>With rounding</b-card-text>\r\n          <b-overlay\r\n            :show=\"show\"\r\n            class=\"d-inline-block\"\r\n            rounded=\"circle\"\r\n          >\r\n            <b-img\r\n              rounded=\"circle\"\r\n              height=\"300\"\r\n              :src=\"require('@/assets/images/banner/banner-27.jpg')\"\r\n              alt=\"Image 1\"\r\n            />\r\n          </b-overlay>\r\n        </b-col>\r\n        <b-col\r\n          md=\"6\"\r\n          class=\"mt-1 mt-md-0\"\r\n        >\r\n          <b-card-text>Without rounding</b-card-text>\r\n          <b-overlay\r\n            :show=\"show\"\r\n            class=\"d-inline-block\"\r\n          >\r\n            <b-img\r\n              rounded=\"circle\"\r\n              height=\"300\"\r\n              :src=\"require('@/assets/images/banner/banner-27.jpg')\"\r\n              alt=\"Image 1\"\r\n            />\r\n          </b-overlay>\r\n        </b-col>\r\n      </b-row>\r\n    </div>\r\n\r\n    <template #code>\r\n      {{ codeCornerRounded }}\r\n    </template>\r\n  </b-card-code>\r\n</template>\r\n\r\n<script>\r\nimport BCardCode from '@core/components/b-card-code'\r\nimport {\r\n  BOverlay, BButton, BImg, BRow, BCol, BCardText,\r\n} from 'bootstrap-vue'\r\nimport Ripple from 'vue-ripple-directive'\r\nimport { codeCornerRounded } from './code'\r\n\r\nexport default {\r\n  components: {\r\n    BCardCode,\r\n    BOverlay,\r\n    BButton,\r\n    BCardText,\r\n    BImg,\r\n    BRow,\r\n    BCol,\r\n  },\r\n  directives: {\r\n    Ripple,\r\n  },\r\n  data() {\r\n    return {\r\n      show: false,\r\n      codeCornerRounded,\r\n    }\r\n  },\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/components/overlay"}]}