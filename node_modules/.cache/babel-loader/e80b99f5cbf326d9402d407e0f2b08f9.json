AAA,YANA;AAOA,MAAA,OAAA,EAAA,OAPA;AAQA,MAAA,WAAA,EAAA,WARA;AASA,MAAA,UAAA,EAAA,UATA;AAUA,MAAA,QAAA,EAAA,QAVA;AAWA,MAAA,cAAA,EAAA,cAXA;AAYA,MAAA,WAAA,EAAA,WAZA;AAaA,MAAA,MAAA,EAAA,MAbA;AAcA,MAAA,aAAA,EAAA,aAdA;AAeA,MAAA,gBAAA,EAAA,gBAfA;AAgBA,MAAA,WAAA,EAAA,WAhBA;AAkBA;AACA,MAAA,UAAA,EAAA,UAnBA;AAqBA;AACA,MAAA,sBAAA,EAAA,sBAtBA;AAuBA,MAAA,mBAAA,EAAA,mBAvBA;AAwBA,MAAA,wBAAA,EAAA,wBAxBA;AA0BA,MAAA,WAAA,EAAA,WA1BA;AA2BA,MAAA,WAAA,EAAA,WA3BA;AA4BA,MAAA,aAAA,EAAA,aA5BA;AA8BA;AACA,MAAA,UAAA,EAAA,UA/BA;AAgCA,MAAA,UAAA,EAAA,UAhCA;AAiCA,MAAA,YAAA,EAAA;AAjCA,KAAA;AAmCA;AAnHA,CAAA","sourcesContent":["<template>\r\n\r\n  <div>\r\n\r\n    <user-list-add-new\r\n      :is-add-new-user-sidebar-active.sync=\"isAddNewUserSidebarActive\"\r\n      :role-options=\"roleOptions\"\r\n      :plan-options=\"planOptions\"\r\n      @refetch-data=\"refetchData\"\r\n    />\r\n\r\n    <!-- Filters -->\r\n    <users-list-filters\r\n      :role-filter.sync=\"roleFilter\"\r\n      :plan-filter.sync=\"planFilter\"\r\n      :status-filter.sync=\"statusFilter\"\r\n      :role-options=\"roleOptions\"\r\n      :plan-options=\"planOptions\"\r\n      :status-options=\"statusOptions\"\r\n    />\r\n\r\n    <!-- Table Container Card -->\r\n    <b-card\r\n      no-body\r\n      class=\"mb-0\">\r\n      <div class=\"m-2\">\r\n        <!-- Table Top -->\r\n        <b-row>\r\n\r\n          <!-- Per Page -->\r\n          <b-col\r\n            cols=\"12\"\r\n            md=\"6\"\r\n            class=\"d-flex align-items-center justify-content-start mb-1 mb-md-0\"\r\n          >\r\n            <label>Show</label>\r\n            <v-select\r\n              v-model=\"perPage\"\r\n              :dir=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\r\n              :options=\"perPageOptions\"\r\n              :clearable=\"false\"\r\n              class=\"per-page-selector d-inline-block mx-50\"\r\n            />\r\n            <label>entries</label>\r\n          </b-col>\r\n\r\n          <!-- Search -->\r\n          <b-col\r\n            cols=\"12\"\r\n            md=\"6\"\r\n          >\r\n            <div class=\"d-flex align-items-center justify-content-end\">\r\n              <b-form-input\r\n                v-model=\"searchQuery\"\r\n                class=\"d-inline-block mr-1\"\r\n                placeholder=\"Search...\"\r\n              />\r\n              <b-button\r\n                variant=\"primary\"\r\n                @click=\"isAddNewUserSidebarActive = true\"\r\n              >\r\n                <span class=\"text-nowrap\">Add User</span>\r\n              </b-button>\r\n            </div>\r\n          </b-col>\r\n        </b-row>\r\n\r\n      </div>\r\n\r\n      <b-table\r\n        ref=\"refUserListTable\"\r\n        class=\"position-relative\"\r\n        :items=\"fetchUsers\"\r\n        responsive\r\n        :fields=\"tableColumns\"\r\n        primary-key=\"id\"\r\n        :sort-by.sync=\"sortBy\"\r\n        show-empty\r\n        empty-text=\"No matching records found\"\r\n        :sort-desc.sync=\"isSortDirDesc\"\r\n      >\r\n\r\n        <!-- Column: User -->\r\n        <template #cell(user)=\"data\">\r\n          <b-media vertical-align=\"center\">\r\n            <template #aside>\r\n              <b-avatar\r\n                size=\"32\"\r\n                :src=\"data.item.avatar\"\r\n                :text=\"avatarText(data.item.fullName)\"\r\n                :variant=\"`light-${resolveUserRoleVariant(data.item.role)}`\"\r\n                :to=\"{ name: 'apps-users-view', params: { id: data.item.id } }\"\r\n              />\r\n            </template>\r\n            <b-link\r\n              :to=\"{ name: 'apps-users-view', params: { id: data.item.id } }\"\r\n              class=\"font-weight-bold d-block text-nowrap\"\r\n            >\r\n              {{ data.item.fullName }}\r\n            </b-link>\r\n            <small class=\"text-muted\">@{{ data.item.username }}</small>\r\n          </b-media>\r\n        </template>\r\n\r\n        <!-- Column: Role -->\r\n        <template #cell(role)=\"data\">\r\n          <div class=\"text-nowrap\">\r\n            <feather-icon\r\n              :icon=\"resolveUserRoleIcon(data.item.role)\"\r\n              size=\"18\"\r\n              class=\"mr-50\"\r\n              :class=\"`text-${resolveUserRoleVariant(data.item.role)}`\"\r\n            />\r\n            <span class=\"align-text-top text-capitalize\">{{ data.item.role }}</span>\r\n          </div>\r\n        </template>\r\n\r\n        <!-- Column: Status -->\r\n        <template #cell(status)=\"data\">\r\n          <b-badge\r\n            pill\r\n            :variant=\"`light-${resolveUserStatusVariant(data.item.status)}`\"\r\n            class=\"text-capitalize\"\r\n          >\r\n            {{ data.item.status }}\r\n          </b-badge>\r\n        </template>\r\n\r\n        <!-- Column: Actions -->\r\n        <template #cell(actions)=\"data\">\r\n          <b-dropdown\r\n            variant=\"link\"\r\n            no-caret\r\n            :right=\"$store.state.appConfig.isRTL\"\r\n          >\r\n\r\n            <template #button-content>\r\n              <feather-icon\r\n                icon=\"MoreVerticalIcon\"\r\n                size=\"16\"\r\n                class=\"align-middle text-body\"\r\n              />\r\n            </template>\r\n            <b-dropdown-item :to=\"{ name: 'apps-users-view', params: { id: data.item.id } }\">\r\n              <feather-icon icon=\"FileTextIcon\" />\r\n              <span class=\"align-middle ml-50\">Details</span>\r\n            </b-dropdown-item>\r\n\r\n            <b-dropdown-item :to=\"{ name: 'apps-users-edit', params: { id: data.item.id } }\">\r\n              <feather-icon icon=\"EditIcon\" />\r\n              <span class=\"align-middle ml-50\">Edit</span>\r\n            </b-dropdown-item>\r\n\r\n            <b-dropdown-item>\r\n              <feather-icon icon=\"TrashIcon\" />\r\n              <span class=\"align-middle ml-50\">Delete</span>\r\n            </b-dropdown-item>\r\n          </b-dropdown>\r\n        </template>\r\n\r\n      </b-table>\r\n      <div class=\"mx-2 mb-2\">\r\n        <b-row>\r\n\r\n          <b-col\r\n            cols=\"12\"\r\n            sm=\"6\"\r\n            class=\"d-flex align-items-center justify-content-center justify-content-sm-start\"\r\n          >\r\n            <span class=\"text-muted\">Showing {{ dataMeta.from }} to {{ dataMeta.to }} of {{ dataMeta.of }} entries</span>\r\n          </b-col>\r\n          <!-- Pagination -->\r\n          <b-col\r\n            cols=\"12\"\r\n            sm=\"6\"\r\n            class=\"d-flex align-items-center justify-content-center justify-content-sm-end\"\r\n          >\r\n\r\n            <b-pagination\r\n              v-model=\"currentPage\"\r\n              :total-rows=\"totalUsers\"\r\n              :per-page=\"perPage\"\r\n              first-number\r\n              last-number\r\n              class=\"mb-0 mt-1 mt-sm-0\"\r\n              prev-class=\"prev-item\"\r\n              next-class=\"next-item\"\r\n            >\r\n              <template #prev-text>\r\n                <feather-icon\r\n                  icon=\"ChevronLeftIcon\"\r\n                  size=\"18\"\r\n                />\r\n              </template>\r\n              <template #next-text>\r\n                <feather-icon\r\n                  icon=\"ChevronRightIcon\"\r\n                  size=\"18\"\r\n                />\r\n              </template>\r\n            </b-pagination>\r\n\r\n          </b-col>\r\n\r\n        </b-row>\r\n      </div>\r\n    </b-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  BCard, BRow, BCol, BFormInput, BButton, BTable, BMedia, BAvatar, BLink,\r\n  BBadge, BDropdown, BDropdownItem, BPagination,\r\n} from 'bootstrap-vue'\r\nimport vSelect from 'vue-select'\r\nimport store from '@/store'\r\nimport { ref, onUnmounted } from '@vue/composition-api'\r\nimport { avatarText } from '@core/utils/filter'\r\nimport UsersListFilters from './UsersListFilters.vue'\r\nimport useUsersList from './useUsersList'\r\n// import userStoreModule from '../userStoreModule'\r\nimport UserListAddNew from './UserListAddNew.vue'\r\n\r\nexport default {\r\n  components: {\r\n    UsersListFilters,\r\n    UserListAddNew,\r\n\r\n    BCard,\r\n    BRow,\r\n    BCol,\r\n    BFormInput,\r\n    BButton,\r\n    BTable,\r\n    BMedia,\r\n    BAvatar,\r\n    BLink,\r\n    BBadge,\r\n    BDropdown,\r\n    BDropdownItem,\r\n    BPagination,\r\n\r\n    vSelect,\r\n  },\r\n  setup() {\r\n    const USER_APP_STORE_MODULE_NAME = 'app-user'\r\n\r\n    // Register module\r\n    // if (!store.hasModule(USER_APP_STORE_MODULE_NAME)) store.registerModule(USER_APP_STORE_MODULE_NAME, userStoreModule)\r\n\r\n    // UnRegister on leave\r\n    onUnmounted(() => {\r\n      if (store.hasModule(USER_APP_STORE_MODULE_NAME)) store.unregisterModule(USER_APP_STORE_MODULE_NAME)\r\n    })\r\n\r\n    const isAddNewUserSidebarActive = ref(false)\r\n\r\n    const roleOptions = [\r\n      { label: 'Admin', value: 'admin' },\r\n      { label: 'Author', value: 'author' },\r\n      { label: 'Editor', value: 'editor' },\r\n      { label: 'Maintainer', value: 'maintainer' },\r\n      { label: 'Subscriber', value: 'subscriber' },\r\n    ]\r\n\r\n    const planOptions = [\r\n      { label: 'Basic', value: 'basic' },\r\n      { label: 'Company', value: 'company' },\r\n      { label: 'Enterprise', value: 'enterprise' },\r\n      { label: 'Team', value: 'team' },\r\n    ]\r\n\r\n    const statusOptions = [\r\n      { label: 'Pending', value: 'pending' },\r\n      { label: 'Active', value: 'active' },\r\n      { label: 'Inactive', value: 'inactive' },\r\n    ]\r\n\r\n    const {\r\n      fetchUsers,\r\n      tableColumns,\r\n      perPage,\r\n      currentPage,\r\n      totalUsers,\r\n      dataMeta,\r\n      perPageOptions,\r\n      searchQuery,\r\n      sortBy,\r\n      isSortDirDesc,\r\n      refUserListTable,\r\n      refetchData,\r\n\r\n      // UI\r\n      resolveUserRoleVariant,\r\n      resolveUserRoleIcon,\r\n      resolveUserStatusVariant,\r\n\r\n      // Extra Filters\r\n      roleFilter,\r\n      planFilter,\r\n      statusFilter,\r\n    } = useUsersList()\r\n\r\n    return {\r\n\r\n      // Sidebar\r\n      isAddNewUserSidebarActive,\r\n\r\n      fetchUsers,\r\n      tableColumns,\r\n      perPage,\r\n      currentPage,\r\n      totalUsers,\r\n      dataMeta,\r\n      perPageOptions,\r\n      searchQuery,\r\n      sortBy,\r\n      isSortDirDesc,\r\n      refUserListTable,\r\n      refetchData,\r\n\r\n      // Filter\r\n      avatarText,\r\n\r\n      // UI\r\n      resolveUserRoleVariant,\r\n      resolveUserRoleIcon,\r\n      resolveUserStatusVariant,\r\n\r\n      roleOptions,\r\n      planOptions,\r\n      statusOptions,\r\n\r\n      // Extra Filters\r\n      roleFilter,\r\n      planFilter,\r\n      statusFilter,\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.per-page-selector {\r\n  width: 90px;\r\n}\r\n</style>\r\n\r\n<style lang=\"scss\">\r\n@import '@core/scss/vue/libs/vue-select.scss';\r\n</style>\r\n"],"sourceRoot":"src/views/apps/user/users-list"}]}