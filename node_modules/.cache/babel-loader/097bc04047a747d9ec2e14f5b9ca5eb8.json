       <b-col md=\"6\">\r\n          <b-form-group label=\"DATE OF BIRTH\" label-for=\"v-useme\">\r\n            <b-form-input id=\"v-useme\" placeholder=\"Enter the date of Birth\" />\r\n          </b-form-group>\r\n        </b-col>\r\n        <b-col md=\"4\">\r\n          <b-form-group label=\"GENDER\" label-for=\"v-1\">\r\n            <v-select\r\n              id=\"v-1\"\r\n              placeholder=\"Please choose ....\"\r\n              :options=\"['Please choose ...', 'Male', 'Female']\"\r\n              label=\"text\"\r\n            />\r\n          </b-form-group>\r\n        </b-col>\r\n        <b-col md=\"4\">\r\n          <b-form-group label=\"MANUAL WORKER/CLERIC\" label-for=\"v-2\">\r\n            <v-select\r\n              id=\"v-2\"\r\n              placeholder=\"Please choose ....\"\r\n              :options=\"['Please choose ...', 'Clerical', 'Manual Worker']\"\r\n              label=\"text\"\r\n            />\r\n          </b-form-group>\r\n        </b-col>\r\n        <b-col md=\"4\">\r\n          <b-form-group label=\"WAITING PERIOD\" label-for=\"v-3\">\r\n            <v-select\r\n              id=\"v-3\"\r\n              placeholder=\"7 Consecutive days\"\r\n              :options=\"[\r\n                '7 Consecutive days',\r\n                '14 Consecutive days',\r\n                '21 Consecutive days',\r\n                '28 Consecutive days',\r\n              ]\"\r\n              label=\"text\"\r\n            />\r\n          </b-form-group>\r\n        </b-col>\r\n        <b-col md=\"4\">\r\n          <b-form-group label=\"BENEFIT PERIOD\" label-for=\"v-4\">\r\n            <v-select\r\n              id=\"v-4\"\r\n              placeholder=\"26 weeks\"\r\n              :options=\"['26 weeks', '52 weeks', '104 weeks', '156 weeks']\"\r\n              label=\"text\"\r\n            />\r\n          </b-form-group>\r\n        </b-col>\r\n        <b-col md=\"6\">\r\n          <b-form-group label=\"SCOPE OF COVER\" label-for=\"v-5\">\r\n            <v-select\r\n              id=\"v-5\"\r\n              placeholder=\"Please choose ..\"\r\n              :options=\"[\r\n                'Please choose ..',\r\n                '24 hours a day/ 365 days a year',\r\n                'working hours only, excluding commuting',\r\n                'working hours only, including direct travel to and from work',\r\n              ]\"\r\n              label=\"text\"\r\n            />\r\n          </b-form-group>\r\n        </b-col>\r\n        <b-col md=\"4\">\r\n          <b-form-group label=\"LUMP SUM (CAPITAL BENEFIT)\" label-for=\"v-6\">\r\n            <v-select\r\n              id=\"v-6\"\r\n              placeholder=\"Please choose ..\"\r\n              :options=\"lumpsum\"\r\n              label=\"text\"\r\n            />\r\n          </b-form-group>\r\n        </b-col>\r\n        <b-col md=\"4\">\r\n          <b-form-group label=\"WEEKLY BENEFIT\" label-for=\"v-7\">\r\n            <v-select\r\n              id=\"v-7\"\r\n              :options=\"weekly\"\r\n              placeholder=\"Please choose ..\"\r\n              label=\"text\"\r\n            />\r\n          </b-form-group>\r\n        </b-col>\r\n        <b-col md=\"4\">\r\n          <b-form-group label=\"WEEKLY COVER:\" label-for=\"v-8\">\r\n            <v-select\r\n              id=\"v-8\"\r\n              placeholder=\"Please choose ..\"\r\n              :options=\"[\r\n                'Please choose ..',\r\n                'Accident Only',\r\n                'Accident and Sickness',\r\n              ]\"\r\n              label=\"text\"\r\n            />\r\n          </b-form-group>\r\n        </b-col>\r\n      </b-row>\r\n    </b-tab>\r\n\r\n    <!-- personal info tab -->\r\n    <b-tab title=\"PROFESSIONAL INDEMNITY\">\r\n      <b-row>\r\n        <b-col cols=\"12\" class=\"mb-2\">\r\n          <b-form-group\r\n            label=\"PROFESSIONAL INDEMNITY LIMIT\"\r\n            label-for=\"v-coeuy\"\r\n          >\r\n            <v-select\r\n              id=\"v-coeuy\"\r\n              placeholder=\"N/A not Taken\"\r\n              :options=\"['N/A not Taken', '$500,000']\"\r\n              label=\"text\"\r\n            />\r\n          </b-form-group>\r\n        </b-col>\r\n        <b-col md=\"6\">\r\n          <b-form-group\r\n            label=\"IS A SEPARATE FEE CHARGED FOR ADVICE?\"\r\n            label-for=\"v-country\"\r\n          >\r\n            <b-form-radio-group class=\"d-flex align-items-center\">\r\n              <label class=\"mr-2\" for=\"ans\"> Yes </label>\r\n              <b-form-radio id=\"yes\" name=\"ans\" value=\"Yes\" :inline=\"true\" />\r\n              <label class=\"mr-2\" for=\"ans_no\"> No </label>\r\n              <b-form-radio id=\"no\" name=\"ans_no\" value=\"No\" :inline=\"true\" />\r\n            </b-form-radio-group>\r\n          </b-form-group>\r\n        </b-col>\r\n        <b-col md=\"6\">\r\n          <b-form-group\r\n            label-for=\"v-language\"\r\n            label=\"NUMBER OF PROPRIETORS / PRINCIPALS GIVING ADVICE\"\r\n          >\r\n            <v-select\r\n              id=\"Language\"\r\n              v-model=\"proprietors\"\r\n              :options=\"noProprietors\"\r\n              label=\"text\"\r\n            />\r\n          </b-form-group>\r\n        </b-col>\r\n      </b-row>\r\n    </b-tab>\r\n\r\n    <!-- address -->\r\n    <b-tab title=\"TOOLS OF TRADE\">\r\n      <b-row>\r\n        <b-col cols=\"12\" md=\"8\" class=\"mb-2\">\r\n          <b-form-group\r\n            label=\"TOOLS OF TRADE - MAXIMUM SUM INSURED\"\r\n            label-for=\"v-couy\"\r\n          >\r\n            <v-select\r\n              id=\"v-couy\"\r\n              placeholder=\"N/A not Taken\"\r\n              :options=\"toolsofTrade\"\r\n              label=\"text\"\r\n            />\r\n          </b-form-group>\r\n        </b-col>\r\n        <b-col cols=\"12\" style=\"width: 400px\">\r\n          <span style=\"width: 100%\">&nbsp;</span>\r\n        </b-col>\r\n      </b-row>\r\n    </b-tab>\r\n\r\n    <!-- LEGALS & TAX AUDIT-->\r\n    <b-tab title=\"LEGALS & TAX AUDIT\">\r\n      <b-row>\r\n        <b-col cols=\"12\" class=\"mb-2\" md=\"8\">\r\n          <b-form-group label=\"LEGALS & TAX AUDIT LIMIT\" label-for=\"v-countty\">\r\n            <v-select\r\n              id=\"v-countty\"\r\n              placeholder=\"N/A not Taken\"\r\n              :options=\"[\r\n                'N/A not Taken',\r\n                '$25,000 limit any one claim, $50,000 aggregate limit',\r\n              ]\"\r\n              label=\"text\"\r\n            />\r\n          </b-form-group>\r\n        </b-col>\r\n        <b-col cols=\"12\" style=\"width: 480px\">\r\n          <span style=\"width: 100%\">&nbsp;</span>\r\n        </b-col>\r\n      </b-row>\r\n    </b-tab>\r\n\r\n    <!-- STATUTORY LIABILITY -->\r\n    <b-tab title=\"STATUTORY LIABILITY\">\r\n      <b-row>\r\n        <b-col cols=\"12\" md=\"8\" class=\"mb-2\">\r\n          <b-form-group label=\"STATUTORY LIABILITY LIMIT\" label-for=\"v-couny\">\r\n            <v-select\r\n              id=\"v-couny\"\r\n              placeholder=\"N/A not Taken\"\r\n              :options=\"['N/A not Taken', '$100,000']\"\r\n              label=\"text\"\r\n            />\r\n          </b-form-group>\r\n        </b-col>\r\n        <b-col cols=\"12\" style=\"width: 500px\">\r\n          <span style=\"width: 100%\">&nbsp;</span>\r\n        </b-col>\r\n      </b-row>\r\n    </b-tab>\r\n  </b-tabs>\r\n</template>\r\n\r\n<script>\r\nimport vSelect from \"vue-select\";\r\nimport {\r\n  BRow,\r\n  BCol,\r\n  BTabs,\r\n  BTab,\r\n  BFormRadio,\r\n  BFormRadioGroup,\r\n  BFormGroup,\r\n  BFormInput,\r\n} from \"bootstrap-vue\";\r\nimport ToastificationContent from \"@core/components/toastification/ToastificationContent.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    BRow,\r\n    BCol,\r\n    BTabs,\r\n    BTab,\r\n    BFormRadio,\r\n    BFormRadioGroup,\r\n    BFormGroup,\r\n    BFormInput,\r\n    vSelect,\r\n    // eslint-disable-next-line vue/no-unused-components\r\n    ToastificationContent,\r\n  },\r\n  data() {\r\n    return {\r\n      proprietors: \" \",\r\n      lumpsum: [\r\n        \"please choose ..\",\r\n        \"$0\",\r\n        \"$50,000\",\r\n        \"$60,000\",\r\n        \"$70,000\",\r\n        \"$80,000\",\r\n        \"$90,000\",\r\n        \"$100,000\",\r\n        \"$110,000\",\r\n        \"$120,000\",\r\n        \"$130,000\",\r\n        \"$140,000\",\r\n        \"$150,000\",\r\n        \"$160,000\",\r\n        \"$170,000\",\r\n        \"$180,000\",\r\n        \"$190,000\",\r\n        \"$200,000\",\r\n        \"$210,000\",\r\n        \"$220,000\",\r\n        \"$230,000\",\r\n        \"$240,000\",\r\n        \"$250,000\",\r\n      ],\r\n      weekly: [\r\n        \"please choose ..\",\r\n        \"$0\",\r\n        \"$350\",\r\n        \"$500\",\r\n        \"$700\",\r\n        \"$800\",\r\n        \"$900\",\r\n        \"$1000\",\r\n        \"$1,250\",\r\n        \"1,500\",\r\n        \"$1,750\",\r\n        \"$2,000\",\r\n        \"2,250\",\r\n        \"$2,500\",\r\n      ],\r\n      noProprietors: [\r\n        { value: \"please choose ..\", text: \"choose number of proprietors\" },\r\n        { value: \"1\", text: \"1\" },\r\n        { value: \"2\", text: \"2\" },\r\n        { value: \"3\", text: \"3\" },\r\n        { value: \"4\", text: \"4\" },\r\n        { value: \"5\", text: \"5\" },\r\n        { value: \"m_five\", text: \"More than 5\" },\r\n      ],\r\n      toolsofTrade: [\r\n        \"N/A Not Taken\",\r\n        \"$5000\",\r\n        \"$6000\",\r\n        \"$7000\",\r\n        \"$8000\",\r\n        \"$9000\",\r\n        \"$10,000\",\r\n        \"$11,000\",\r\n        \"$12,000\",\r\n        \"$13,000\",\r\n        \"$14,000\",\r\n        \"$15,000\",\r\n        \"$16,000\",\r\n        \"$17,000\",\r\n        \"$18,000\",\r\n        \"$19,000\",\r\n        \"$20,000\",\r\n        \"$21,000\",\r\n        \"$22,000\",\r\n        \"$23,000\",\r\n        \"$24,000\",\r\n        \"$25,000\",\r\n      ],\r\n    };\r\n  },\r\n  methods: {\r\n    formSubmitted() {\r\n      this.$toast({\r\n        component: ToastificationContent,\r\n        props: {\r\n          title: \"Form Submitted\",\r\n          icon: \"EditIcon\",\r\n          variant: \"success\",\r\n        },\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/dashboard/quote"}]}