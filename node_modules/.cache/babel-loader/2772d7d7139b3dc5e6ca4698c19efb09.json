     },\r\n      })\r\n    },\r\n    onError() {\r\n      this.$toast({\r\n        component: ToastificationContent,\r\n        props: {\r\n          title: 'Failed to copy texts!',\r\n          icon: 'BellIcon',\r\n        },\r\n      })\r\n    },\r\n  },\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/extensions/clipboard"}]}