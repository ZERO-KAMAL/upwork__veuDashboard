A,IAAA;AACA,KAHA;AAIA,IAAA,SAJA,uBAIA;AACA,WAAA,KAAA,CAAA,UAAA,EAAA,IAAA;AACA,KANA;AAOA,IAAA,WAPA,yBAOA;AACA;AACA;AACA,WAAA,KAAA,CAAA,UAAA,EAAA,MAAA,CAAA,aAAA;AACA;AAXA;AAfA,CAAA","sourcesContent":["<template>\r\n  <b-card-code title=\"Modal Method\">\r\n    <b-card-text class=\"mb-0\">\r\n      <span>You can access modal using </span>\r\n      <code>ref</code>\r\n      <span> attribute and then call the </span>\r\n      <code>show(), hide()</code>\r\n      <span> or </span>\r\n      <code>toggle()</code>\r\n      <span> methods.</span>\r\n    </b-card-text>\r\n\r\n    <!-- modal trigger button -->\r\n    <div class=\"demo-inline-spacing\">\r\n      <b-button\r\n        id=\"show-btn\"\r\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n        variant=\"outline-primary\"\r\n        @click=\"showModal\"\r\n      >\r\n        Open Modal\r\n      </b-button>\r\n      <b-button\r\n        id=\"toggle-btn\"\r\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n        variant=\"outline-primary\"\r\n        @click=\"toggleModal\"\r\n      >\r\n        Toggle Modal\r\n      </b-button>\r\n    </div>\r\n\r\n    <!-- modal -->\r\n    <b-modal\r\n      ref=\"my-modal\"\r\n      hide-footer\r\n      title=\"Using Component Methods\"\r\n    >\r\n      <div class=\"d-block text-center\">\r\n        <h3>Hello From My Modal!</h3>\r\n      </div>\r\n      <b-button\r\n        v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\r\n        class=\"mt-3\"\r\n        variant=\"outline-secondary\"\r\n        block\r\n        @click=\"hideModal\"\r\n      >\r\n        Close Me\r\n      </b-button>\r\n      <b-button\r\n        v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\r\n        class=\"mt-2\"\r\n        variant=\"outline-primary\"\r\n        block\r\n        @click=\"toggleModal\"\r\n      >\r\n        Toggle Me\r\n      </b-button>\r\n    </b-modal>\r\n\r\n    <template #code>\r\n      {{ codeMethod }}\r\n    </template>\r\n  </b-card-code>\r\n</template>\r\n\r\n<script>\r\nimport { BButton, BModal, BCardText } from 'bootstrap-vue'\r\nimport BCardCode from '@core/components/b-card-code'\r\nimport Ripple from 'vue-ripple-directive'\r\nimport { codeMethod } from './code'\r\n\r\nexport default {\r\n  components: {\r\n    BCardCode,\r\n    BButton,\r\n    BModal,\r\n    BCardText,\r\n  },\r\n  directives: {\r\n    Ripple,\r\n  },\r\n  data() {\r\n    return {\r\n      codeMethod,\r\n    }\r\n  },\r\n  methods: {\r\n    showModal() {\r\n      this.$refs['my-modal'].show()\r\n    },\r\n    hideModal() {\r\n      this.$refs['my-modal'].hide()\r\n    },\r\n    toggleModal() {\r\n      // We pass the ID of the button that we want to return focus to\r\n      // when the modal has hidden\r\n      this.$refs['my-modal'].toggle('#toggle-btn')\r\n    },\r\n  },\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/components/modal"}]}