AATA;AAZA,CAAA","sourcesContent":["<template>\r\n  <b-card-code title=\"Disable dates\">\r\n    <b-card-text>\r\n      <span>If you need to disable specific dates within the date picker, specify a function reference to the </span>\r\n      <code>date-disabled-fn</code>\r\n      <span> prop.</span>\r\n    </b-card-text>\r\n\r\n    <b-form-datepicker\r\n      v-model=\"value\"\r\n      :date-disabled-fn=\"dateDisabled\"\r\n      locale=\"en\"\r\n    />\r\n\r\n    <template #code>\r\n      {{ codeDisableDate }}\r\n    </template>\r\n  </b-card-code>\r\n</template>\r\n\r\n<script>\r\nimport BCardCode from '@core/components/b-card-code'\r\nimport { BFormDatepicker, BCardText } from 'bootstrap-vue'\r\nimport { codeDisableDate } from './code'\r\n\r\nexport default {\r\n  components: {\r\n    BCardCode,\r\n    BCardText,\r\n    BFormDatepicker,\r\n  },\r\n  data() {\r\n    return {\r\n      value: '',\r\n      codeDisableDate,\r\n    }\r\n  },\r\n  methods: {\r\n    dateDisabled(ymd, date) {\r\n      // Disable weekends (Sunday = `0`, Saturday = `6`) and\r\n      // disable days that fall on the 13th of the month\r\n      const weekday = date.getDay()\r\n      const day = date.getDate()\r\n\r\n      // Return `true` if the date should be disabled\r\n      return weekday === 0 || weekday === 6 || day === 13\r\n    },\r\n  },\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/forms/form-element/form-datepicker"}]}