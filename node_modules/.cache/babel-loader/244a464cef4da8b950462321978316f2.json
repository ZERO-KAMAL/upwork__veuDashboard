AA,EAAA;AADA,GAXA;AAcA,EAAA,IAdA,kBAcA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,YADA;AAEA,MAAA,IAAA,EAAA,KAFA;AAGA,MAAA,OAAA,EAAA,IAHA;AAIA,MAAA,aAAA,EAAA;AAJA,KAAA;AAMA,GArBA;AAsBA,EAAA,aAtBA,2BAsBA;AACA,SAAA,YAAA;AACA,GAxBA;AAyBA,EAAA,OAAA,EAAA;AACA,IAAA,YADA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,kBACA;AACA,UAAA,KAAA,OAAA,EAAA;AACA,QAAA,YAAA,CAAA,KAAA,OAAA,CAAA;AACA,aAAA,OAAA,GAAA,IAAA;AACA;AACA,KANA;AAOA,IAAA,UAPA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA,gBAOA,QAPA,EAOA;AAAA;;AACA,WAAA,YAAA;AACA,WAAA,OAAA,GAAA,UAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,YAAA;;AACA,QAAA,QAAA;AACA,OAHA,EAGA,IAHA,CAAA;AAIA,KAbA;AAcA,IAAA,QAdA,sBAcA;AACA;AACA,WAAA,KAAA,CAAA,MAAA,CAAA,KAAA;AACA,KAjBA;AAkBA,IAAA,OAlBA,qBAkBA;AAAA;;AACA,WAAA,IAAA,GAAA,IAAA,CADA,CAEA;;AACA,WAAA,UAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,KAAA;AACA,OAFA;AAGA;AAxBA;AAzBA,CAAA","sourcesContent":["<template>\r\n  <b-card-code title=\"Busy state input group\">\r\n    <b-card-text>In this example, we obscure the input and button:</b-card-text>\r\n    <div>\r\n      <b-overlay\r\n        :show=\"busy\"\r\n        rounded=\"lg\"\r\n        opacity=\"0.6\"\r\n        @hidden=\"onHidden\"\r\n      >\r\n        <template #overlay>\r\n          <div class=\"d-flex align-items-center\">\r\n            <b-spinner\r\n              small\r\n              type=\"grow\"\r\n              variant=\"secondary\"\r\n            />\r\n            <b-spinner\r\n              type=\"grow\"\r\n              variant=\"dark\"\r\n            />\r\n            <b-spinner\r\n              small\r\n              type=\"grow\"\r\n              variant=\"secondary\"\r\n            />\r\n\r\n            <!-- We add an SR only text for screen readers -->\r\n            <span class=\"sr-only\">Please wait...</span>\r\n          </div>\r\n        </template>\r\n\r\n        <b-input-group\r\n          size=\"lg\"\r\n          :aria-hidden=\"busy ? 'true' : null\"\r\n        >\r\n          <b-form-input\r\n            v-model=\"value\"\r\n            :disabled=\"busy\"\r\n          />\r\n          <b-input-group-append>\r\n            <b-button\r\n              ref=\"button\"\r\n              v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\r\n              :disabled=\"busy\"\r\n              variant=\"primary\"\r\n              @click=\"onClick\"\r\n            >\r\n              Do something\r\n            </b-button>\r\n          </b-input-group-append>\r\n        </b-input-group>\r\n      </b-overlay>\r\n    </div>\r\n\r\n    <template #code>\r\n      {{ codeBusyState }}\r\n    </template>\r\n  </b-card-code>\r\n</template>\r\n\r\n<script>\r\nimport BCardCode from '@core/components/b-card-code'\r\nimport {\r\n  BOverlay, BInputGroup, BFormInput, BInputGroupAppend, BButton, BSpinner, BCardText,\r\n} from 'bootstrap-vue'\r\nimport Ripple from 'vue-ripple-directive'\r\nimport { codeBusyState } from './code'\r\n\r\nexport default {\r\n  components: {\r\n    BOverlay,\r\n    BInputGroup,\r\n    BFormInput,\r\n    BInputGroupAppend,\r\n    BButton,\r\n    BCardText,\r\n    BSpinner,\r\n    BCardCode,\r\n  },\r\n  directives: {\r\n    Ripple,\r\n  },\r\n  data() {\r\n    return {\r\n      value: 'Some value',\r\n      busy: false,\r\n      timeout: null,\r\n      codeBusyState,\r\n    }\r\n  },\r\n  beforeDestroy() {\r\n    this.clearTimeout()\r\n  },\r\n  methods: {\r\n    clearTimeout() {\r\n      if (this.timeout) {\r\n        clearTimeout(this.timeout)\r\n        this.timeout = null\r\n      }\r\n    },\r\n    setTimeout(callback) {\r\n      this.clearTimeout()\r\n      this.timeout = setTimeout(() => {\r\n        this.clearTimeout()\r\n        callback()\r\n      }, 5000)\r\n    },\r\n    onHidden() {\r\n      // Return focus to the button\r\n      this.$refs.button.focus()\r\n    },\r\n    onClick() {\r\n      this.busy = true\r\n      // Simulate an async request\r\n      this.setTimeout(() => {\r\n        this.busy = false\r\n      })\r\n    },\r\n  },\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/components/overlay"}]}