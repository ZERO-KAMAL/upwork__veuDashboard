            bar: {\r\n              columnWidth: '17%',\r\n              endingShape: 'rounded',\r\n            },\r\n            distributed: true,\r\n          },\r\n          yaxis: {\r\n            labels: {\r\n              style: {\r\n                colors: '#6E6B7B',\r\n                fontSize: '0.86rem',\r\n                fontFamily: 'Montserrat',\r\n              },\r\n            },\r\n          },\r\n        },\r\n      },\r\n      // budget chart\r\n      budgetChart: {\r\n        options: {\r\n          chart: {\r\n            height: 80,\r\n            toolbar: { show: false },\r\n            zoom: { enabled: false },\r\n            type: 'line',\r\n            sparkline: { enabled: true },\r\n          },\r\n          stroke: {\r\n            curve: 'smooth',\r\n            dashArray: [0, 5],\r\n            width: [2],\r\n          },\r\n          colors: [$themeColors.primary, '#dcdae3'],\r\n          tooltip: {\r\n            enabled: false,\r\n          },\r\n        },\r\n      },\r\n    }\r\n  },\r\n}\r\n</script>\r\n"],"sourceRoot":"src/views/dashboard/ecommerce"}]}