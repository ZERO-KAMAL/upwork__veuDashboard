n      <label for=\"sb-locales\">Locale</label>\r\n      <b-form-select\r\n        id=\"sb-locales\"\r\n        v-model=\"locale\"\r\n        :options=\"locales\"\r\n      />\r\n      <label\r\n        for=\"sb-local\"\r\n        class=\"mt-2\"\r\n      >Spin button with locale</label>\r\n      <b-form-spinbutton\r\n        id=\"sb-locale\"\r\n        v-model=\"value\"\r\n        :locale=\"locale\"\r\n        min=\"0\"\r\n        max=\"10\"\r\n        step=\"0.125\"\r\n      />\r\n      <b-card-text class=\"mt-1\">\r\n        Value: {{ value }}\r\n      </b-card-text>\r\n    </div>\r\n\r\n    <template #code>\r\n      {{ codeLocale }}\r\n    </template>\r\n  </b-card-code>\r\n</template>\r\n\r\n<script>\r\nimport BCardCode from '@core/components/b-card-code'\r\nimport { BFormSpinbutton, BFormSelect, BCardText } from 'bootstrap-vue'\r\nimport { codeLocale } from './code'\r\n\r\nexport default {\r\n  components: {\r\n    BFormSpinbutton,\r\n    BCardCode,\r\n    BFormSelect,\r\n    BCardText,\r\n  },\r\n  data() {\r\n    return {\r\n      value: 0,\r\n      locale: 'fr-CA',\r\n      locales: [\r\n        { value: 'en', text: 'English' },\r\n        { value: 'de', text: 'German' },\r\n        { value: 'fr-CA', text: 'French (Canadian)' },\r\n        { value: 'fa', text: 'Persian' },\r\n        { value: 'ar-EG', text: 'Arabic (Egyptian)' },\r\n      ],\r\n      codeLocale,\r\n    }\r\n  },\r\n}\r\n</script>\r\n"]}]}