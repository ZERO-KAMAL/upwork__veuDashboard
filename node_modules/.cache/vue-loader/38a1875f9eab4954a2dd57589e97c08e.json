tr'\"\r\n                :selectable=\"option => ! option.value.includes('select_value')\"\r\n                :options=\"languageName\"\r\n                label=\"text\"\r\n              />\r\n            </b-form-group>\r\n          </b-col>\r\n        </b-row>\r\n      </tab-content>\r\n\r\n      <!-- address -->\r\n      <tab-content title=\"Address\">\r\n        <b-row>\r\n          <b-col\r\n            cols=\"12\"\r\n            class=\"mb-2\"\r\n          >\r\n            <h5 class=\"mb-0\">\r\n              Address\r\n            </h5>\r\n            <small class=\"text-muted\">Enter Your Address.</small>\r\n          </b-col>\r\n          <b-col md=\"6\">\r\n            <b-form-group\r\n              label=\"Address\"\r\n              label-for=\"v-address\"\r\n            >\r\n              <b-form-input\r\n                id=\"v-address\"\r\n                placeholder=\"98 Borough bridge Road, Birmingham\"\r\n              />\r\n            </b-form-group>\r\n          </b-col>\r\n          <b-col md=\"6\">\r\n            <b-form-group\r\n              label=\"Landmark\"\r\n              label-for=\"v-landmark\"\r\n            >\r\n              <b-form-input\r\n                id=\"v-landmark\"\r\n                placeholder=\"Borough bridge\"\r\n              />\r\n            </b-form-group>\r\n          </b-col>\r\n          <b-col md=\"6\">\r\n            <b-form-group\r\n              label=\"Pincode\"\r\n              label-for=\"v-pincode\"\r\n            >\r\n              <b-form-input\r\n                id=\"v-pincode\"\r\n                placeholder=\"658921\"\r\n              />\r\n            </b-form-group>\r\n          </b-col>\r\n          <b-col md=\"6\">\r\n            <b-form-group\r\n              label=\"City\"\r\n              label-for=\"v-city\"\r\n            >\r\n              <b-form-input\r\n                id=\"v-city\"\r\n                placeholder=\"Birmingham\"\r\n              />\r\n            </b-form-group>\r\n      