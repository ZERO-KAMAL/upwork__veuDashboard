-shop\"\r\n        :class=\"{'show': mqShallShowLeftSidebar}\"\r\n      >\r\n        <b-row>\r\n          <b-col cols=\"12\">\r\n            <h6 class=\"filter-heading d-none d-lg-block\">\r\n              Filters\r\n            </h6>\r\n          </b-col>\r\n        </b-row>\r\n\r\n        <!-- Filters' Card -->\r\n        <b-card>\r\n\r\n          <!-- Multi Range -->\r\n          <div class=\"multi-range-price\">\r\n            <h6 class=\"filter-title mt-0\">\r\n              Multi Range\r\n            </h6>\r\n            <b-form-radio-group\r\n              v-model=\"filters.priceRangeDefined\"\r\n              class=\"price-range-defined-radio-group\"\r\n              stacked\r\n              :options=\"filterOptions.priceRangeDefined\"\r\n            />\r\n          </div>\r\n\r\n          <!-- Price Slider -->\r\n          <div class=\"price-slider\">\r\n            <h6 class=\"filter-title\">\r\n              Price Range\r\n            </h6>\r\n            <vue-slider\r\n              v-model=\"filters.priceRange\"\r\n              :direction=\"$store.state.appConfig.isRTL ? 'rtl' : 'ltr'\"\r\n            />\r\n          </div>\r\n\r\n          <!-- Categories -->\r\n          <div class=\"product-categories\">\r\n            <h6 class=\"filter-title\">\r\n              Categories\r\n            </h6>\r\n            <b-form-radio-group\r\n              v-model=\"filters.categories\"\r\n              class=\"categories-radio-group\"\r\n              stacked\r\n              :options=\"filterOptions.categories\"\r\n            />\r\n          </div>\r\n\r\n          <!-- Brands -->\r\n          <div class=\"brands\">\r\n            <h6 class=\"filter-title\">\r\n              Brands\r\n            </h6>\r\n            <b-form-radio-group\r\n              v-model=\"filters.brands\"\r\n              class=\"brands-radio-group\"\r\n              stacked\r\n              :options=\"filterOptions.brands\"\r\n            />\r\n          </div>\r\n\r\n          <!-- Ratings -->\r\n          <div class=\"ratings\">\r\n            <h6 class=\"filter-title\">\r\n              Ratings\r\n            </h6>\r\n            <div\r\n              v-for=\"rating in filterOptions.ratings\"\r\n              :key=\"rating.rating\"\r\n              class=\"ratings-list\"\r\n            >\r\n              <b-link>\r\n                <div class=\"d-flex\">\r\n                  <feather-icon\r\n                    v-for=\"star in 5\"\r\n                    :key=\"star\"\r\n                    icon=\"StarIcon\"\r\n                    size=\"17\"\r\n                    :class=\"[{'fill-current': star <= rating.rating}, star <= rating.rating ? 'text-warning' : 'text-muted']\"\r\n                  />\r\n                  <span class=\"ml-25\">&amp; up</span>\r\n                </div>\r\n              </b-link>\r\n              <div class=\"stars-received\">\r\n                <span>{{ rating.count }}</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </b-card>\r\n      </div>\r\n    </div>\r\n\r\n    <div\r\n      class=\"body-content-overlay\"\r\n      :class=\"{'show': mqShallShowLeftSidebar}\"\r\n      @click=\"$emit('update:mq-shall-show-left-sidebar', false)\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  BRow, BCol, BCard, BFormRadioGroup, BLink,\r\n} from 'bootstrap-vue'\r\nimport VueSlider from 'vue-slider-component'\r\n\r\nexport default {\r\n  components: {\r\n    BRow,\r\n    BCol,\r\n    BFormRadioGroup,\r\n    BLink,\r\n    BCard,\r\n\r\n    // 3rd Party\r\n    VueSlider,\r\n  },\r\n  props: {\r\n    filters: {\r\n      type: Object,\r\n      required: true,\r\n    },\r\n    filterOptions: {\r\n      type: Object,\r\n      required: true,\r\n    },\r\n    mqShallShowLeftSidebar: {\r\n      type: Boolean,\r\n      required: true,\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n@import '~@core/scss/vue/libs/vue-slider.scss';\r\n</style>\r\n\r\n<style lang=\"scss\" scoped>\r\n.categories-radio-group,\r\n.brands-radio-group,\r\n.price-range-defined-radio-group {\r\n    ::v-deep > .custom-control {\r\n    margin-bottom: 0.75rem;\r\n  }\r\n}\r\n</style>\r\n"]}]}