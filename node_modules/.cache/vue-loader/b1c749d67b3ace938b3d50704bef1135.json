enter</code>\r\n      <span> , </span>\r\n      <code>bottom-left</code>.\r\n    </b-card-text>\r\n\r\n    <h5 class=\"mb-0\">\r\n      Top Positions\r\n    </h5>\r\n    <div class=\"demo-inline-spacing\">\r\n\r\n      <!-- Top right -->\r\n      <b-button\r\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n        variant=\"outline-primary\"\r\n        @click=\"showToast('primary','top-right')\"\r\n      >\r\n        Top Right\r\n      </b-button>\r\n\r\n      <!-- Top left -->\r\n      <b-button\r\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n        variant=\"outline-primary\"\r\n        @click=\"showToast('secondary','top-left')\"\r\n      >\r\n        Top Left\r\n      </b-button>\r\n\r\n      <!-- top center -->\r\n      <b-button\r\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n        variant=\"outline-primary\"\r\n        @click=\"showToast('success','top-center')\"\r\n      >\r\n        Top Center\r\n      </b-button>\r\n    </div>\r\n\r\n    <h5 class=\"mt-2 mb-0\">\r\n      Bottom Positions\r\n    </h5>\r\n    <div class=\"demo-inline-spacing\">\r\n      <!-- bottom right -->\r\n      <b-button\r\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n        variant=\"outline-primary\"\r\n        @click=\"showToast('danger','bottom-right')\"\r\n      >\r\n        Bottom Right\r\n      </b-button>\r\n\r\n      <!-- bottom left -->\r\n      <b-button\r\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n        variant=\"outline-primary\"\r\n        @click=\"showToast('warning','bottom-left')\"\r\n      >\r\n        Bottom Left\r\n      </b-button>\r\n\r\n      <!-- bottom center -->\r\n      <b-button\r\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n        variant=\"outline-primary\"\r\n        @click=\"showToast('info','bottom-center')\"\r\n      >\r\n        Bottom Center\r\n      </b-button>\r\n    </div>\r\n    <template #code>\r\n      {{ codePosition }}\r\n    </template>\r\n  </b-card-code>\r\n</template>\r\n\r\n<script>\r\nimport { BButton, BCardText } from 'bootstrap-vue'\r\nimport BCardCode from '@core/components/b-card-code/BCardCode.vue'\r\nimport ToastificationContent from '@core/components/toastification/ToastificationContent.vue'\r\nimport Ripple from 'vue-ripple-directive'\r\nimport { codePosition } from './code'\r\n\r\nexport default {\r\n  components: {\r\n    BButton,\r\n    BCardCode,\r\n    BCardText,\r\n    // eslint-disable-next-line vue/no-unused-components\r\n    ToastificationContent,\r\n  },\r\n  directives: {\r\n    Ripple,\r\n  },\r\n  data() {\r\n    return {\r\n      codePosition,\r\n    }\r\n  },\r\n  methods: {\r\n    showToast(variant, position) {\r\n      this.$toast({\r\n        component: ToastificationContent,\r\n        props: {\r\n          title: 'Notification',\r\n          icon: 'InfoIcon',\r\n          text: 'I do not think that word means what you think it means.',\r\n          variant,\r\n        },\r\n      },\r\n      {\r\n        position,\r\n      })\r\n    },\r\n  },\r\n}\r\n</script>\r\n"]}]}