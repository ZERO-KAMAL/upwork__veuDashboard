om 'bootstrap-vue'\r\nimport Ripple from 'vue-ripple-directive'\r\nimport { codeTarget } from './code'\r\n\r\nexport default {\r\n  components: {\r\n    BCardCode,\r\n    BButton,\r\n    BCardText,\r\n  },\r\n  directives: {\r\n    Ripple,\r\n  },\r\n  data() {\r\n    return {\r\n      counter: 0,\r\n      codeTarget,\r\n    }\r\n  },\r\n  methods: {\r\n    toast(toaster, append = false) {\r\n      this.counter += 1\r\n      this.$bvToast.toast(`Toast ${this.counter} body content`, {\r\n        title: `Toaster ${toaster}`,\r\n        toaster,\r\n        solid: false,\r\n        appendToast: append,\r\n      })\r\n    },\r\n  },\r\n}\r\n</script>\r\n"]}]}