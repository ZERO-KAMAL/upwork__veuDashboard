n      >\r\n        <div\r\n          class=\"d-flex\"\r\n          dir=\"ltr\"\r\n        >\r\n          <b-button\r\n            v-if=\"value\"\r\n            size=\"sm\"\r\n            variant=\"outline-secondary\"\r\n            @click=\"clearDate\"\r\n          >\r\n            Clear date\r\n          </b-button>\r\n          <b-button\r\n            size=\"sm\"\r\n            variant=\"outline-primary\"\r\n            class=\"ml-auto\"\r\n            @click=\"setToday\"\r\n          >\r\n            Set Today\r\n          </b-button>\r\n        </div>\r\n      </b-calendar>\r\n    </div>\r\n\r\n    <template #code>\r\n      {{ codeSlot }}\r\n    </template>\r\n  </b-card-code>\r\n</template>\r\n\r\n<script>\r\nimport BCardCode from '@core/components/b-card-code'\r\nimport { BCalendar, BButton, BCardText } from 'bootstrap-vue'\r\nimport { codeSlot } from './code'\r\n\r\nexport default {\r\n  components: {\r\n    BCardCode,\r\n    BCalendar,\r\n    BButton,\r\n    BCardText,\r\n  },\r\n  data() {\r\n    return {\r\n      value: null,\r\n      codeSlot,\r\n    }\r\n  },\r\n  methods: {\r\n    setToday() {\r\n      const now = new Date()\r\n      this.value = new Date(now.getFullYear(), now.getMonth(), now.getDate())\r\n    },\r\n    clearDate() {\r\n      this.value = ''\r\n    },\r\n  },\r\n}\r\n</script>\r\n"]}]}