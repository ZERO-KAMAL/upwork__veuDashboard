ax\"\r\n        >\r\n          <b-progress-bar :value=\"value\">\r\n            <strong>{{ value.toFixed(2) }} / {{ max }}</strong>\r\n          </b-progress-bar>\r\n        </b-progress>\r\n      </div>\r\n\r\n      <div>\r\n        <label>Custom label via property</label>\r\n        <b-progress\r\n          :max=\"max\"\r\n          class=\"progress-bar-info\"\r\n        >\r\n          <b-progress-bar\r\n            :value=\"value\"\r\n            :label=\"`${((value / max) * 100).toFixed(2)}%`\"\r\n            variant=\"info\"\r\n          />\r\n        </b-progress>\r\n      </div>\r\n\r\n      <div>\r\n        <label>Custom label via property (HTML support)</label>\r\n        <b-progress\r\n          :max=\"max\"\r\n          class=\"progress-bar-warning\"\r\n        >\r\n          <b-progress-bar\r\n            :value=\"value\"\r\n            :label-html=\"`<del>${value}</del>`\"\r\n            variant=\"warning\"\r\n          />\r\n        </b-progress>\r\n      </div>\r\n    </div>\r\n\r\n    <template #code>\r\n      {{ codeCustomLabel }}\r\n    </template>\r\n  </b-card-code>\r\n</template>\r\n\r\n<script>\r\nimport BCardCode from '@core/components/b-card-code'\r\nimport { BProgress, BProgressBar, BCardText } from 'bootstrap-vue'\r\nimport { codeCustomLabel } from './code'\r\n\r\nexport default {\r\n  components: {\r\n    BCardCode,\r\n    BProgress,\r\n    BProgressBar,\r\n    BCardText,\r\n  },\r\n  data() {\r\n    return {\r\n      codeCustomLabel,\r\n      value: 33.333333333,\r\n      max: 50,\r\n    }\r\n  },\r\n}\r\n</script>\r\n"]}]}