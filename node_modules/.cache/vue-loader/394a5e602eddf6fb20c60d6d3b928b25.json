ata\"\r\n        :marks=\"true\"\r\n        class=\"mb-3\"\r\n        :direction=\"direction\"\r\n      />\r\n      <vue-slider\r\n        v-model=\"value3\"\r\n        :adsorb=\"true\"\r\n        :marks=\"marks\"\r\n        :included=\"true\"\r\n        class=\"mb-2\"\r\n        :direction=\"direction\"\r\n      />\r\n    </div>\r\n\r\n    <template #code>\r\n      {{ codeAdsorb }}\r\n    </template>\r\n  </b-card-code>\r\n</template>\r\n\r\n<script>\r\nimport { BCardText } from 'bootstrap-vue'\r\nimport BCardCode from '@core/components/b-card-code/BCardCode.vue'\r\nimport VueSlider from 'vue-slider-component'\r\nimport store from '@/store/index'\r\nimport { codeAdsorb } from './code'\r\n\r\nexport default {\r\n  components: {\r\n    BCardText,\r\n    VueSlider,\r\n    BCardCode,\r\n  },\r\n  data() {\r\n    return {\r\n      codeAdsorb,\r\n      value1: 20,\r\n      value2: 'c',\r\n      value3: 60,\r\n      data: ['a', 'b', 'c', 'd', 'e', 'f', 'g'],\r\n      marks: [0, 10, 30, 60, 100],\r\n      dir: 'ltr',\r\n    }\r\n  },\r\n  computed: {\r\n    direction() {\r\n      if (store.state.appConfig.isRTL) {\r\n        // eslint-disable-next-line vue/no-side-effects-in-computed-properties\r\n        this.dir = 'rtl'\r\n        return this.dir\r\n      }\r\n      // eslint-disable-next-line vue/no-side-effects-in-computed-properties\r\n      this.dir = 'ltr'\r\n      return this.dir\r\n    },\r\n  },\r\n}\r\n</script>\r\n"]}]}