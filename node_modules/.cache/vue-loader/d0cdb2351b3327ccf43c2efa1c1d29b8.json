\r\n  >\r\n\r\n    <!-- content -->\r\n    <div class=\"blog-detail-wrapper\">\r\n      <b-row>\r\n        <!-- blogs -->\r\n        <b-col cols=\"12\">\r\n          <b-card\r\n            :img-src=\"blogDetail.blog.img\"\r\n            img-top\r\n            img-alt=\"Blog Detail Pic\"\r\n            :title=\"blogDetail.blog.title\"\r\n          >\r\n            <b-media no-body>\r\n              <b-media-aside\r\n                vertical-align=\"center\"\r\n                class=\"mr-50\"\r\n              >\r\n                <b-avatar\r\n                  href=\"javascript:void(0)\"\r\n                  size=\"24\"\r\n                  :src=\"blogDetail.blog.avatar\"\r\n                />\r\n              </b-media-aside>\r\n              <b-media-body>\r\n                <small class=\"text-muted mr-50\">by</small>\r\n                <small>\r\n                  <b-link class=\"text-body\">{{ blogDetail.blog.userFullName }}</b-link>\r\n                </small>\r\n                <span class=\"text-muted ml-75 mr-50\">|</span>\r\n                <small class=\"text-muted\">{{ blogDetail.blog.createdTime }}</small>\r\n              </b-media-body>\r\n            </b-media>\r\n            <div class=\"my-1 py-25\">\r\n              <b-link\r\n                v-for=\"tag in blogDetail.blog.tags\"\r\n                :key=\"tag\"\r\n              >\r\n                <b-badge\r\n                  pill\r\n                  class=\"mr-75\"\r\n                  :variant=\"tagsColor(tag)\"\r\n                >\r\n                  {{ tag }}\r\n                </b-badge>\r\n              </b-link>\r\n            </div>\r\n            <!-- eslint-disable vue/no-v-html -->\r\n            <div\r\n              class=\"blog-content\"\r\n              v-html=\"blogDetail.blog.content\"\r\n            />\r\n\r\n            <!-- user commnets -->\r\n            <b-media\r\n              v-for=\"user in blogDetail.blog.UserComment\"\r\n              :key=\"user.avatar\"\r\n              no-body\r\n            >\r\n              <b-media-aside>\r\n                <b-avatar\r\n                  size=\"60\"\r\n                  :src=\"user.avatar\"\r\n                />\r\n              </b-media-aside>\r\n              <b-media-body>\r\n                <h6 class=\"font-weight-bolder\">\r\n                  {{ user.fullName }}\r\n                </h6>\r\n                <b-card-text>\r\n                  {{ user.comment }}\r\n                </b-card-text>\r\n              </b-media-body>\r\n            </b-media>\r\n            <!-- eslint-enable -->\r\n            <hr class=\"my-2\">\r\n\r\n            <div class=\"d-flex align-items-center justify-content-between\">\r\n              <div class=\"d-flex align-items-center\">\r\n                <div class=\"d-flex align-items-center mr-1\">\r\n                  <b-link class=\"mr-50\">\r\n                    <feather-icon\r\n                      icon=\"MessageSquareIcon\"\r\n                      size=\"21\"\r\n                      class=\"text-body\"\r\n                    />\r\n                  </b-link>\r\n                  <b-link>\r\n                    <div class=\"text-body\">\r\n                      {{ kFormatter(blogDetail.blog.comments) }}\r\n                    </div>\r\n                  </b-link>\r\n                </div>\r\n                <div class=\"d-flex align-items-center\">\r\n                  <b-link class=\"mr-50\">\r\n                    <feather-icon\r\n                      size=\"21\"\r\n                      icon=\"BookmarkIcon\"\r\n                      class=\"text-body\"\r\n                    />\r\n                  </b-link>\r\n                  <b-link>\r\n                    <div class=\"text-body\">\r\n                      {{ kFormatter(blogDetail.blog.bookmarked) }}\r\n                    </div>\r\n                  </b-link>\r\n                </div>\r\n              </div>\r\n\r\n              <!-- dropdown -->\r\n              <div class=\"blog-detail-share\">\r\n                <b-dropdown\r\n                  variant=\"link\"\r\n                  toggle-class=\"p-0\"\r\n                  no-caret\r\n                  right\r\n                >\r\n                  <template #button-content>\r\n                    <feather-icon\r\n                      size=\"21\"\r\n                      icon=\"Share2Icon\"\r\n                      class=\"text-body\"\r\n                    />\r\n                  </template>\r\n                  <b-dropdown-item\r\n                    v-for=\"icon in socialShareIcons\"\r\n                    :key=\"icon\"\r\n                    href=\"#\"\r\n                  >\r\n                    <feather-icon\r\n                      :icon=\"icon\"\r\n                      size=\"18\"\r\n                    />\r\n                  </b-dropdown-item>\r\n                </b-dropdown>\r\n              </div>\r\n              <!--/ dropdown -->\r\n            </div>\r\n          </b-card>\r\n        </b-col>\r\n        <!--/ blogs -->\r\n\r\n        <!-- blog comment -->\r\n        <b-col\r\n          id=\"blogComment\"\r\n          cols=\"12\"\r\n          class=\"mt-2\"\r\n        >\r\n          <h6 class=\"section-label\">\r\n            Comment\r\n          </h6>\r\n          <b-card\r\n            v-for=\"(comment,index) in blogDetail.comments\"\r\n            :key=\"index\"\r\n          >\r\n            <b-media no-body>\r\n              <b-media-aside class=\"mr-75\">\r\n                <b-avatar\r\n                  :src=\"comment.avatar\"\r\n                  size=\"38\"\r\n                />\r\n              </b-media-aside>\r\n              <b-media-body>\r\n                <h6 class=\"font-weight-bolder mb-25\">\r\n                  {{ comment.userFullName }}\r\n                </h6>\r\n                <b-card-text>{{ comment.commentedAt }}</b-card-text>\r\n                <b-card-text>\r\n                  {{ comment.commentText }}\r\n                </b-card-text>\r\n                <b-link>\r\n                  <div class=\"d-inline-flex align-items-center\">\r\n                    <feather-icon\r\n                      icon=\"CornerUpLeftIcon\"\r\n                      size=\"18\"\r\n                      class=\"mr-50\"\r\n                    />\r\n                    <span>Reply</span>\r\n                  </div>\r\n                </b-link>\r\n              </b-media-body>\r\n            </b-media>\r\n          </b-card>\r\n        </b-col>\r\n        <!--/ blog comment -->\r\n\r\n        <!-- Leave a Blog Comment -->\r\n        <b-col\r\n          cols=\"12\"\r\n          class=\"mt-2\"\r\n        >\r\n          <h6 class=\"section-label\">\r\n            Leave a Comment\r\n          </h6>\r\n          <b-card>\r\n            <b-form>\r\n              <b-row>\r\n                <b-col sm=\"6\">\r\n                  <b-form-group class=\"mb-2\">\r\n                    <b-form-input\r\n                      name=\"name\"\r\n                      placeholder=\"Name\"\r\n                    />\r\n                  </b-form-group>\r\n                </b-col>\r\n                <b-col sm=\"6\">\r\n                  <b-form-group class=\"mb-2\">\r\n                    <b-form-input\r\n                      name=\"email\"\r\n                      type=\"email\"\r\n                      placeholder=\"Email\"\r\n                    />\r\n                  </b-form-group>\r\n                </b-col>\r\n                <b-col sm=\"6\">\r\n                  <b-form-group class=\"mb-2\">\r\n                    <b-form-input\r\n                      name=\"website\"\r\n                      placeholder=\"Website\"\r\n                    />\r\n                  </b-form-group>\r\n                </b-col>\r\n                <b-col cols=\"12\">\r\n                  <b-form-group class=\"mb-2\">\r\n                    <b-form-textarea\r\n                      name=\"textarea\"\r\n                      rows=\"4\"\r\n                      placeholder=\"Website\"\r\n                    />\r\n                  </b-form-group>\r\n                </b-col>\r\n                <b-col cols=\"12\">\r\n                  <b-form-checkbox\r\n                    id=\"checkbox-1\"\r\n                    v-model=\"commentCheckmark\"\r\n                    name=\"checkbox-1\"\r\n                    class=\"mb-2\"\r\n                  >\r\n                    Save my name, email, and website in this browser for the next time I comment.\r\n                  </b-form-checkbox>\r\n                </b-col>\r\n                <b-col cols=\"12\">\r\n                  <b-button\r\n                    v-ripple.400=\"'rgba(255, 255, 255, 0.15)'\"\r\n                    variant=\"primary\"\r\n                  >\r\n                    Post Comment\r\n                  </b-button>\r\n                </b-col>\r\n              </b-row>\r\n            </b-form>\r\n          </b-card>\r\n        </b-col>\r\n        <!--/ Leave a Blog Comment -->\r\n      </b-row>\r\n      <!--/ blogs -->\r\n    </div>\r\n    <!--/ content -->\r\n\r\n    <!-- sidebar -->\r\n    <div\r\n      slot=\"sidebar\"\r\n      class=\"blog-sidebar py-2 py-lg-0\"\r\n    >\r\n      <!-- input search -->\r\n      <b-form-group class=\"blog-search\">\r\n        <b-input-group class=\"input-group-merge\">\r\n          <b-form-input\r\n            id=\"search-input\"\r\n            v-model=\"search_query\"\r\n            placeholder=\"Search here\"\r\n          />\r\n          <b-input-group-append\r\n            class=\"cursor-pointer\"\r\n            is-text\r\n          >\r\n            <feather-icon\r\n              icon=\"SearchIcon\"\r\n            />\r\n          </b-input-group-append>\r\n        </b-input-group>\r\n      </b-form-group>\r\n      <!--/ input search -->\r\n\r\n      <!-- recent posts -->\r\n      <div class=\"blog-recent-posts mt-3\">\r\n        <h6 class=\"section-label mb-75\">\r\n          Recent Posts\r\n        </h6>\r\n        <b-media\r\n          v-for=\"(recentpost,index) in blogSidebar.recentPosts\"\r\n          :key=\"recentpost.img\"\r\n          no-body\r\n          :class=\"index? 'mt-2':''\"\r\n        >\r\n          <b-media-aside class=\"mr-2\">\r\n            <b-link>\r\n              <b-img\r\n                :src=\"recentpost.img\"\r\n                :alt=\"recentpost.img.slice(6)\"\r\n                width=\"100\"\r\n                rounded\r\n                height=\"70\"\r\n              />\r\n            </b-link>\r\n          </b-media-aside>\r\n          <b-media-body>\r\n            <h6 class=\"blog-recent-post-title\">\r\n              <b-link class=\"text-body-heading\">\r\n                {{ recentpost.title }}\r\n              </b-link>\r\n            </h6>\r\n            <span class=\"text-muted mb-0\">\r\n              {{ recentpost.createdTime }}\r\n            </span>\r\n          </b-media-body>\r\n        </b-media>\r\n      </div>\r\n      <!--/ recent posts -->\r\n\r\n      <!-- categories -->\r\n      <div class=\"blog-categories mt-3\">\r\n        <h6 class=\"section-label mb-1\">\r\n          Categories\r\n        </h6>\r\n\r\n        <div\r\n          v-for=\"category in blogSidebar.categories\"\r\n          :key=\"category.icon\"\r\n          class=\"d-flex justify-content-start align-items-center mb-75\"\r\n        >\r\n          <b-link>\r\n            <b-avatar\r\n              rounded\r\n              size=\"32\"\r\n              :variant=\"tagsColor(category.category)\"\r\n              class=\"mr-75\"\r\n            >\r\n              <feather-icon\r\n                :icon=\"category.icon\"\r\n                size=\"16\"\r\n              />\r\n            </b-avatar>\r\n          </b-link>\r\n          <b-link>\r\n            <div class=\"blog-category-title text-body\">\r\n              {{ category.category }}\r\n            </div>\r\n          </b-link>\r\n        </div>\r\n      </div>\r\n      <!--/ categories -->\r\n    </div>\r\n  </content-with-sidebar>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  BFormInput, BMedia, BAvatar, BMediaAside, BMediaBody, BImg, BLink, BFormGroup, BInputGroup, BInputGroupAppend,\r\n  BCard, BRow, BCol, BBadge, BCardText, BDropdown, BDropdownItem, BForm, BFormTextarea, BFormCheckbox, BButton,\r\n} from 'bootstrap-vue'\r\nimport Ripple from 'vue-ripple-directive'\r\nimport { kFormatter } from '@core/utils/filter'\r\nimport ContentWithSidebar from '@core/layouts/components/content-with-sidebar/ContentWithSidebar.vue'\r\n\r\nexport default {\r\n  components: {\r\n    BFormInput,\r\n    BMedia,\r\n    BAvatar,\r\n    BMediaAside,\r\n    BMediaBody,\r\n    BLink,\r\n    BCard,\r\n    BRow,\r\n    BCol,\r\n    BFormGroup,\r\n    BInputGroup,\r\n    BInputGroupAppend,\r\n    BImg,\r\n    BBadge,\r\n    BCardText,\r\n    BDropdown,\r\n    BForm,\r\n    BDropdownItem,\r\n    BFormTextarea,\r\n    BFormCheckbox,\r\n    BButton,\r\n    ContentWithSidebar,\r\n  },\r\n  directives: {\r\n    Ripple,\r\n  },\r\n  data() {\r\n    return {\r\n      search_query: '',\r\n      commentCheckmark: '',\r\n      blogDetail: [],\r\n      blogSidebar: {},\r\n      socialShareIcons: [\r\n        'GithubIcon',\r\n        'GitlabIcon',\r\n        'FacebookIcon',\r\n        'TwitterIcon',\r\n        'LinkedinIcon',\r\n      ],\r\n    }\r\n  },\r\n  created() {\r\n    this.$http.get('/blog/list/data/detail').then(res => { this.blogDetail = res.data })\r\n    this.$http.get('/blog/list/data/sidebar').then(res => { this.blogSidebar = res.data })\r\n  },\r\n  methods: {\r\n    kFormatter,\r\n    tagsColor(tag) {\r\n      if (tag === 'Quote') return 'light-info'\r\n      if (tag === 'Gaming') return 'light-danger'\r\n      if (tag === 'Fashion') return 'light-primary'\r\n      if (tag === 'Video') return 'light-warning'\r\n      if (tag === 'Food') return 'light-success'\r\n      return 'light-primary'\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n@import '@core/scss/vue/pages/page-blog.scss';\r\n</style>\r\n"]}]}