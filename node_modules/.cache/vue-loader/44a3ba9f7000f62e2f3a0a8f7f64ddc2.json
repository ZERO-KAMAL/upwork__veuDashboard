89\"\r\n        @keyup=\"validateInput\"\r\n      />\r\n    </b-form-group>\r\n\r\n    <!-- alert -->\r\n    <b-alert\r\n      v-height-fade.appear\r\n      variant=\"danger\"\r\n      :show=\"show\"\r\n      class=\"mb-0\"\r\n    >\r\n      <div class=\"alert-body\">\r\n        <feather-icon\r\n          icon=\"InfoIcon\"\r\n          class=\"mr-50\"\r\n        />\r\n        the value is invalid you can only enter numbers\r\n      </div>\r\n    </b-alert>\r\n\r\n    <template #code>\r\n      {{ codeExample }}\r\n    </template>\r\n  </b-card-code>\r\n</template>\r\n\r\n<script>\r\nimport BCardCode from '@core/components/b-card-code'\r\nimport {\r\n  BAlert, BFormGroup, BFormInput, BCardText,\r\n} from 'bootstrap-vue'\r\nimport { heightFade } from '@core/directives/animations'\r\nimport { codeExample } from './code'\r\n\r\nexport default {\r\n  components: {\r\n    BCardCode,\r\n    BAlert,\r\n    BFormGroup,\r\n    BFormInput,\r\n    BCardText,\r\n  },\r\n  directives: {\r\n    'height-fade': heightFade,\r\n  },\r\n  data() {\r\n    return {\r\n      value: '',\r\n      show: true,\r\n      codeExample,\r\n    }\r\n  },\r\n  methods: {\r\n    validateInput() {\r\n      if (/^\\d+$/.test(this.value)) {\r\n        this.show = false\r\n      } else {\r\n        this.show = true\r\n      }\r\n    },\r\n  },\r\n}\r\n</script>\r\n"]}]}