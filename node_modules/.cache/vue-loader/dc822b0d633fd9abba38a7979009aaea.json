, BRow, BCol, BAvatar,\r\n} from 'bootstrap-vue'\r\nimport FaqQuestionAnswer from './FaqQuestionAnswer.vue'\r\n\r\nexport default {\r\n  components: {\r\n    BForm,\r\n    BCard,\r\n    BRow,\r\n    BCol,\r\n    BAvatar,\r\n    BCardBody,\r\n    BInputGroup,\r\n    BFormInput,\r\n    BCardText,\r\n    BInputGroupPrepend,\r\n    BTabs,\r\n    BTab,\r\n    BImg,\r\n    FaqQuestionAnswer,\r\n  },\r\n  data() {\r\n    return {\r\n      faqSearchQuery: '',\r\n      faqData: {},\r\n    }\r\n  },\r\n  watch: {\r\n    faqSearchQuery: {\r\n      immediate: true,\r\n      handler() {\r\n        this.fetchData()\r\n      },\r\n    },\r\n  },\r\n  methods: {\r\n    fetchData() {\r\n      this.$http.get('/faq/data', { params: { q: this.faqSearchQuery } }).then(res => { this.faqData = res.data })\r\n    },\r\n  },\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n@import '@core/scss/vue/pages/page-faq.scss';\r\n</style>\r\n"]}]}