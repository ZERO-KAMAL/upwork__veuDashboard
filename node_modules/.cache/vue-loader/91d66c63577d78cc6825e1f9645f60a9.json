  Show popover\r\n      </b-button>\r\n\r\n      <b-popover\r\n        target=\"popover-button-1\"\r\n        placement=\"top\"\r\n        triggers=\"click\"\r\n      >\r\n        <template #title>\r\n          Show Method Popover\r\n        </template>\r\n        <span>Fruitcake candy cheesecake jelly beans cake gummies. Cotton candy I love sweet roll jujubes pastry cake halvah cake.</span>\r\n      </b-popover>\r\n\r\n      <b-button\r\n        id=\"popover-button-2\"\r\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n        variant=\"outline-primary\"\r\n        @click=\"hidePopover\"\r\n      >\r\n        Hide popover\r\n      </b-button>\r\n\r\n      <b-popover\r\n        target=\"popover-button-2\"\r\n        placement=\"top\"\r\n        triggers=\"hover\"\r\n      >\r\n        <template #title>\r\n          Hide Method Popover\r\n        </template>\r\n        <span>Fruitcake candy cheesecake jelly beans cake gummies. Cotton candy I love sweet roll jujubes pastry cake halvah cake.</span>\r\n      </b-popover>\r\n\r\n      <b-button\r\n        id=\"popover-button-3\"\r\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n        variant=\"outline-primary\"\r\n        @click=\"show = !show\"\r\n      >\r\n        Toggle popover\r\n      </b-button>\r\n\r\n      <b-popover\r\n        :show.sync=\"show\"\r\n        target=\"popover-button-3\"\r\n        placement=\"top\"\r\n        triggers=\"click\"\r\n      >\r\n        <template #title>\r\n          Toggle Method Popover\r\n        </template>\r\n        <span>Fruitcake candy cheesecake jelly beans cake gummies. Cotton candy I love sweet roll jujubes pastry cake halvah cake.</span>\r\n      </b-popover>\r\n\r\n      <b-button\r\n        id=\"popover-button-disable\"\r\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n        variant=\"outline-primary\"\r\n      >\r\n        Disable/Enable popover\r\n      </b-button>\r\n\r\n      <b-button\r\n        v-ripple.400=\"'rgba(113, 102, 240, 0.15)'\"\r\n        :variant=\"disabled ? 'success':'danger'\"\r\n        @click=\"disabled = !disabled\"\r\n      >\r\n        {{ disabled ? 'Enable' : 'Disable' }} popover by prop\r\n      </b-button>\r\n\r\n      <b-popover\r\n        ref=\"popover\"\r\n        :disabled.sync=\"disabled\"\r\n        target=\"popover-button-disable\"\r\n      >\r\n        <template #title>\r\n          Disable/Enable Popover\r\n        </template>\r\n        <span>Fruitcake candy cheesecake jelly beans cake gummies. Cotton candy I love sweet roll jujubes pastry cake halvah cake.</span>\r\n      </b-popover>\r\n    </div>\r\n\r\n    <template #code>\r\n      {{ codeMethod }}\r\n    </template>\r\n  </b-card-code>\r\n</template>\r\n\r\n<script>\r\nimport BCardCode from '@core/components/b-card-code'\r\nimport {\r\n  VBPopover, BButton, BPopover, BCardText,\r\n} from 'bootstrap-vue'\r\nimport Ripple from 'vue-ripple-directive'\r\nimport { codeMethod } from './code'\r\n\r\nexport default {\r\n  components: {\r\n    BCardCode,\r\n    BPopover,\r\n    BButton,\r\n    BCardText,\r\n  },\r\n  directives: {\r\n    'b-popover': VBPopover,\r\n    Ripple,\r\n  },\r\n  data() {\r\n    return {\r\n      show: false,\r\n      disabled: false,\r\n      codeMethod,\r\n    }\r\n  },\r\n  methods: {\r\n    showPopover() {\r\n      this.$root.$emit('bv::show::popover', 'popover-button-1')\r\n    },\r\n    hidePopover() {\r\n      this.$root.$emit('bv::hide::popover', 'popover-button-2')\r\n    },\r\n  },\r\n}\r\n</script>\r\n"]}]}